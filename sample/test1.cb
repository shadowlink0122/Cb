int calc() {
    print "calc";
    int a = 2;
    a = a + 1;
    print a;
    int b = 2;
    print ((a + a) * b > 10);
    print ((a - a) * b > 10);
    print (b * (a * a) > 10);
    print (b * (a / a) > 10);
}


void test_bool_and_expr() {
    print "== bool/number";
    print (true == 1); // 1
    print (false == 0); // 1
    print (true == 0); // 0
    print (false == 1); // 0
    print true; // 1
    print false; // 0

    print "比較演算子";
    print (3 > 2); // 1
    print (2 > 3); // 0
    print (2 >= 2); // 1
    print (2 >= 3); // 0
    print (2 < 3); // 1
    print (3 < 2); // 0
    print (2 <= 2); // 1
    print (3 <= 2); // 0

    print "括弧の優先順位";
    print 1 + 2 * 3; // 7
    print (1 + 2) * 3; // 9
    print (1 + (2 * 3)); // 7
    print ((1 + 2) * 3); // 9

    print "NOT演算子";
    print !true; // 0
    print !false; // 1
    print !(1 > 2); // 1
    print !(2 > 1); // 0
    print !0; // 1
    print !1; // 0

    print "複合式";
    print (true && !false); // 1
    print (false || !false); // 1
    print (true && false || true); // 1
    print !(true && false) || (false || true); // 1
    print !((1 + 2) > 3); // 1
    print ((1 < 2) && (2 < 3)) || false; // 1
}

