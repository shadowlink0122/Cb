int main(){
    // 基本的な複数宣言
    int a = 1, b = 2, c = 3;
    println("Basic:", a, b, c);
    
    // 文字列型の複数宣言
    string x = "hello", y = "world";
    println("String:", x, y);
    
    // 異なる初期化パターン
    long p = 100, q = 200, r = 300;
    println("Long:", p, q, r);
    
    // 一部のみ初期化（残りはデフォルト値）
    short s = 5, t, u = 10;
    println("Short:", s, t, u);
}
