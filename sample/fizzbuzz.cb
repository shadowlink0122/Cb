void fizzbuzz(){
    for(tiny i = 0;i <= 30;i = i + 1){
        // && 左から順に実行されるので、&&でバグる
        // print i % 3 == 0 && i % 5 == 0;
        if ((i % 3 == 0) && (i % 5 == 0)) print "FizzBuzz";
        else if (i % 3 == 0) print "Fizz";
        else if (i % 5 == 0) print "Buzz";
        else print i;
    }
}

int main() {
    fizzbuzz();
    
    return 0;
}
