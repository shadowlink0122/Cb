// ネストされたtypedef構造体メンバーアクセスのデバッグ

typedef struct {
    int x;
    int y;
} Point;

typedef struct {
    Point pos;
    int radius;
} Circle;

int main() {
    println("Test 1: Creating Circle with nested literal");
    Circle c = {{10, 20}, 30};
    println("Circle created");
    
    println("\nTest 2: Accessing nested members");
    println("Radius: %d", c.radius);
    
    // 問題の箇所：ネストされた構造体メンバーへのアクセス
    println("Trying to access c.pos.x...");
    int px = c.pos.x;
    println("Position X: %d", px);
    
    return 0;
}
