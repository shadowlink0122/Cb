// Regression test: Const pointer safety
// Previously, modifying const pointee was not caught
// This test ensures const correctness is enforced

func int main() {
    int value = 42;
    const int* ptr = &value;
    
    // This should be an error (trying to modify through const pointer)
    // *ptr = 100;  // Uncomment to test error detection
    
    // But we can read through const pointer
    int x = *ptr;
    println(x);  // Should print 42
    
    // We can also reassign the pointer itself (it's not const)
    int value2 = 99;
    ptr = &value2;
    int y = *ptr;
    println(y);  // Should print 99
    
    return 0;
}
