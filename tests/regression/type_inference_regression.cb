// Regression test: Type inference with complex expressions
// Ensures type inference works correctly in nested expressions

func int main() {
    // Mixed type arithmetic should infer correct types
    tiny t = 10;
    short s = 20;
    int i = 30;
    long l = 40;
    
    // Should infer to long (highest precision)
    auto result1 = t + s + i + l;
    println(result1);  // 100
    
    // With floating point
    float f = 1.5;
    double d = 2.5;
    
    // Should infer to double
    auto result2 = f + d;
    println(result2);  // 4.0
    
    // Complex expression
    auto result3 = (t * s) + (i * 2) - l;
    println(result3);  // 220
    
    return 0;
}
