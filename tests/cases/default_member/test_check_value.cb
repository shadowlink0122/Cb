// デフォルトメンバーへの暗黙的代入のテスト(値確認版)

struct String {
    default string value;
};

void main() {
    String s = {"Hello"};
    println("Before: ");
    println(s.value);  // Hello
    
    println("Assigning World...");
    s = "World";
    
    println("After (explicit): ");
    println(s.value);  // World (期待)
}
