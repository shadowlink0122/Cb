// デフォルトメンバーへの暗黙的代入のテスト(デバッグ版)

struct String {
    default string value;
};

void main() {
    String s = {"Hello"};
    println("Before: ");
    println(s.value);  // Hello
    
    println("Assigning World...");
    // 暗黙的代入: s.value = "World" として解釈されるべき
    s = "World";
    
    println("After: ");
    println(s);  // World (期待)
}
