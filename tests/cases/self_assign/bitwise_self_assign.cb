int main() {
    // ビット演算を使った自己代入テスト
    int x = 12;  // 1100 in binary
    int y = 5;   // 0101 in binary
    
    println(x);  // 12
    println(y);  // 5
    
    // ビット AND 自己代入
    x = x & y;   // 12 & 5 = 1100 & 0101 = 0100 = 4
    println(x);  // 4
    
    // ビット OR 自己代入
    y = y | x;   // 5 | 4 = 0101 | 0100 = 0101 = 5
    println(y);  // 5
    
    // ビット XOR 自己代入
    x = x ^ y;   // 4 ^ 5 = 0100 ^ 0101 = 0001 = 1
    println(x);  // 1
    
    // 左シフト自己代入
    x = x << 2;  // 1 << 2 = 0001 << 2 = 0100 = 4
    println(x);  // 4
    
    // 右シフト自己代入
    y = y >> 1;  // 5 >> 1 = 0101 >> 1 = 0010 = 2
    println(y);  // 2
    
    return 0;
}
