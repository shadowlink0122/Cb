int main() {
    // 変数を使った自己代入テスト
    int x = 50;
    int y = x;
    
    x = x + y; // 50 + 50 = 100
    println(x); // 100
    
    y = x + y; // 100 + 50 = 150
    x = y + x; // 150 + 100 = 250
    y = x - y; // 250 - 150 = 100
    x = x - y; // 250 - 100 = 150
    x = x + y; // 150 + 100 = 250
    y = x - y; // 250 - 100 = 150
    x = x - y; // 250 - 150 = 100
    
    println(x); // 100
    println(y); // 150
    
    return 0;
}
