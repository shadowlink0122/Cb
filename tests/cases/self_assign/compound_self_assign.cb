int main() {
    // 複合代入演算子を使った自己代入テスト
    int x = 10;
    int y = 3;
    
    println(x);  // 10
    println(y);  // 3
    
    // 加算複合代入
    x += y;      // x = x + y = 10 + 3 = 13
    println(x);  // 13
    
    // 減算複合代入
    y -= 1;      // y = y - 1 = 3 - 1 = 2
    println(y);  // 2
    
    // 乗算複合代入
    x *= y;      // x = x * y = 13 * 2 = 26
    println(x);  // 26
    
    // 除算複合代入
    x /= y;      // x = x / y = 26 / 2 = 13
    println(x);  // 13
    
    // 剰余複合代入
    x %= 5;      // x = x % 5 = 13 % 5 = 3
    println(x);  // 3
    
    // ビット演算複合代入
    y &= 2;      // y = y & 2 = 2 & 2 = 2
    println(y);  // 2
    
    x |= 4;      // x = x | 4 = 3 | 4 = 7
    println(x);  // 7
    
    x ^= 1;      // x = x ^ 1 = 7 ^ 1 = 6
    println(x);  // 6
    
    x <<= 1;     // x = x << 1 = 6 << 1 = 12
    println(x);  // 12
    
    x >>= 2;     // x = x >> 2 = 12 >> 2 = 3
    println(x);  // 3
    
    return 0;
}
