enum Job {
    a = 0,
    b,
    c,
    d = 10,
    e
};

int main() {
    int test_a = Job::a;  // 0
    int test_b = Job::b;  // 1
    int test_c = Job::c;  // 2  
    int test_d = Job::d;  // 10
    int test_e = Job::e;  // 11
    
    // Verify enum value assignments
    if (test_a != 0) {
        println("ERROR: Job::a should be 0, got {test_a}");
        return 1;
    }
    
    if (test_b != 1) {
        println("ERROR: Job::b should be 1, got {test_b}");
        return 1;
    }
    
    if (test_c != 2) {
        println("ERROR: Job::c should be 2, got {test_c}");
        return 1;
    }
    
    if (test_d != 10) {
        println("ERROR: Job::d should be 10, got {test_d}");
        return 1;
    }
    
    if (test_e != 11) {
        println("ERROR: Job::e should be 11, got {test_e}");
        return 1;
    }
    
    println("Basic enum values: a={test_a}, b={test_b}, c={test_c}, d={test_d}, e={test_e}");
    println("Basic enum test passed");
    return 0;
}
