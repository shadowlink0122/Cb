// エラーテスト: const T* 経由で値を変更しようとする
include "stdio.cb"

int main() {
    println("=== Test: Cannot modify through pointer to const ===");
    int x = 10;
    const int* ptr = &x;
    
    // エラー: const int* 経由では値を変更できない
    *ptr = 20;  // This should fail
    
    println("ERROR: This should not execute");
    return 0;
}
