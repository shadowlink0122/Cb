// 行コメントとブロックコメントの混在テスト

/*
   プログラムの説明:
   このテストは様々なコメントパターンをテストします
*/

void main() {
    // 行コメント
    int x = 10; /* ブロックコメント */
    println("x = {x}");

    /* ブロックコメント */ int y = 20; // 行コメント
    println("y = {y}");

    // 式の中のブロックコメント
    int z = /* コメント */ 30 /* コメント */ + /* コメント */ 5;
    println("z = {z}");

    /*
       ブロックコメントの後に
       行コメント
    */
    // 続く行コメント
    int result = x + y + z;
    println("result = {result}");

    // コメントアウトされたコード
    /* int unused = 100; */
    // int unused2 = 200;
}
