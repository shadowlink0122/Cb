void main() {
    println("=== Double Array Test ===");
    
    // doubleの配列を作成
    void* arr = new double[5];
    
    // array_set_doubleで値を設定
    array_set_double(arr, 0, 3.14);
    array_set_double(arr, 1, 2.71);
    array_set_double(arr, 2, 1.41);
    
    println("✅ Set 3 double values");
    
    // array_get_doubleで値を取得
    double v0 = array_get_double(arr, 0);
    double v1 = array_get_double(arr, 1);
    double v2 = array_get_double(arr, 2);
    
    println("v0={v0}, v1={v1}, v2={v2}");
    
    if (v0 > 3.13 && v0 < 3.15) {
        println("✅ array_get_double works!");
    } else {
        println("❌ array_get_double failed: got {v0}, expected 3.14");
    }
    
    delete arr;
}
