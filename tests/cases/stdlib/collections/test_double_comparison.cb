// double比較のテスト

int main() {
    println("=== Double Comparison Test ===");
    
    double d1 = 3.14;
    double d2 = 2.71;
    
    println("d1 = {d1}");
    println("d2 = {d2}");
    
    bool test1 = d1 > d2;
    bool test2 = d1 < d2;
    bool test3 = d1 > 3.0;
    bool test4 = d1 < 4.0;
    bool test5 = d1 > 3.13;
    bool test6 = d1 < 3.15;
    bool test7 = 3.14 > 3.13;
    bool test8 = 3.14 < 3.15;
    
    println("d1 > d2: {test1}");
    println("d1 < d2: {test2}");
    println("d1 > 3.0: {test3}");
    println("d1 < 4.0: {test4}");
    println("d1 > 3.13: {test5}");
    println("d1 < 3.15: {test6}");
    println("3.14 > 3.13: {test7}");
    println("3.14 < 3.15: {test8}");
    
    if (d1 > d2) {
        println("✅ d1 > d2 works");
    } else {
        println("❌ d1 > d2 failed");
    }
    
    bool test_and1 = d1 > 3.0;
    bool test_and2 = d1 < 4.0;
    println("test_and1 (d1 > 3.0): {test_and1}");
    println("test_and2 (d1 < 4.0): {test_and2}");
    
    bool combined = test_and1 && test_and2;
    println("combined (test_and1 && test_and2): {combined}");
    
    if (test_and1 && test_and2) {
        println("✅ Range check works");
    } else {
        println("❌ Range check failed");
    }
    
    return 0;
}
