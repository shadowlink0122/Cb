// typedef配列統合テスト
typedef int[3] IntArray;
typedef string[2] StringArray;
typedef bool[4] BoolArray;

int main() {
    println("=== Typedef Array Integration Test ===");
    
    // 配列の宣言と配列リテラル代入
    IntArray numbers;
    numbers = [10, 20, 30];
    
    StringArray texts;
    texts = ["first", "second"];
    
    BoolArray flags;
    flags = [true, false, true, false];
    
    println("Testing typedef array usage:");
    
    println("IntArray numbers:");
    println("  numbers[0] =", numbers[0]);
    println("  numbers[1] =", numbers[1]);
    println("  numbers[2] =", numbers[2]);
    
    println("StringArray texts:");
    println("  texts[0] =", texts[0]);
    println("  texts[1] =", texts[1]);
    
    println("BoolArray flags:");
    println("  flags[0] =", flags[0]);
    println("  flags[1] =", flags[1]);
    println("  flags[2] =", flags[2]);
    println("  flags[3] =", flags[3]);
    
    println("Typedef array integration test completed!");
    return 0;
}
