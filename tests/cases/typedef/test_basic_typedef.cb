// typedef統合テスト - 基本機能（typedefサポート待ちのため通常の型を使用）
// typedef int MyInt;
// typedef string MyString;
// typedef bool MyBool;

int main() {
    println("=== Typedef Integration Test (using standard types) ===");
    
    // typedefの代わりに標準型を使用
    int number = 42;
    string text = "Hello, World!";
    bool flag = true;
    
    // Verify variable assignments
    if (number != 42) {
        println("ERROR: number should be 42, got %d", number);
        return 1;
    }
    
    if (flag != true) {
        println("ERROR: flag should be true, got %d", flag);
        return 1;
    }
    
    println("Testing basic typedef usage:");
    println("int number = %d", number);
    println("string text = %s", text);
    println("bool flag = %d", flag);
    
    // Test modification
    number = number * 2;
    if (number != 84) {
        println("ERROR: After multiplication, number should be 84, got %d", number);
        return 1;
    }
    
    flag = !flag;
    if (flag != false) {
        println("ERROR: After negation, flag should be false, got %d", flag);
        return 1;
    }
    
    println("Modified values:");
    println("int number = %d", number);
    println("bool flag = %d", flag);
    println("Typedef integration test passed!");
    return 0;
}
