// typedef配列定義で定数を使用するテストケース
const int SIZE = 10;
typedef int[SIZE] IntArray;

int main() {
    IntArray arr;
    
    // 配列初期化
    for (int i = 0; i < SIZE; i++) {
        arr[i] = i * 2;
    }
    
    // 配列内容の値検証
    for (int i = 0; i < SIZE; i++) {
        int expected = i * 2;
        if (arr[i] != expected) {
            println("ERROR: arr[%d] should be %d, got %d", i, expected, arr[i]);
            return 1;
        }
    }
    
    // 配列内容確認（出力は最後に）
    for (int i = 0; i < SIZE; i++) {
        println("%d %d", i, arr[i]);
    }
    
    println("Typedef array constant test passed");
    return 0;
}
