// typedef配列の高度なテストケース
const int ROWS = 3;
const int COLS = 4;

typedef int[COLS] Row;

Row createRow(int start) {
    Row row;
    for (int i = 0; i < COLS; i++) {
        row[i] = start + i;
    }
    return row;
}

int main() {
    // 各行をテスト
    for (int r = 0; r < ROWS; r++) {
        Row row = createRow(r * 10);
        for (int c = 0; c < COLS; c++) {
            println("row[", c, "] =", row[c]);
        }
    }
    
    return 0;
}
