// Enum typedef function test
typedef enum { SUCCESS = 0, ERROR = 1 } StatusCode;

StatusCode process_data(int value) {
    if (value > 0) {
        return StatusCode::SUCCESS;
    }
    return StatusCode::ERROR;
}

StatusCode check_status(StatusCode r) {
    if (r == StatusCode::SUCCESS) {
        return StatusCode::SUCCESS;
    }
    return StatusCode::ERROR;
}

int main() {
    StatusCode r1 = process_data(10);
    StatusCode r2 = check_status(process_data(-5));
    
    println("Function result:", r1);
    println("Status check:", r2);
    
    return 0;
}
