// 構造体配列メンバー初期化のテスト

typedef struct {
    int x;
    int y;
} Point;

typedef struct {
    Point[3] vertices;
} Triangle;

int main() {
    println("Test: Creating Triangle with Point array member");
    Triangle tri;
    
    println("Setting vertices manually...");
    tri.vertices[0].x = 1;
    tri.vertices[0].y = 2;
    
    println("vertices[0]: (%d, %d)", tri.vertices[0].x, tri.vertices[0].y);
    
    println("All tests passed!");
    return 0;
}
