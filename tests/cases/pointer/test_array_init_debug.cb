// Array initialization debugging test
// This test is to identify the array initialization bug

int main() {
    println("=== Array Initialization Debug Test ===");
    
    // Test 1: Simple integer array
    println("\nTest 1: Simple integer array");
    int[3] arr1 = [10, 20, 30];
    println("arr1[0] = ", arr1[0], " (expected: 10)");
    println("arr1[1] = ", arr1[1], " (expected: 20)");
    println("arr1[2] = ", arr1[2], " (expected: 30)");
    
    // Test 2: Array with 5 elements
    println("\nTest 2: Array with 5 elements");
    int[5] arr2 = [1, 2, 3, 4, 5];
    println("arr2[0] = ", arr2[0], " (expected: 1)");
    println("arr2[1] = ", arr2[1], " (expected: 2)");
    println("arr2[2] = ", arr2[2], " (expected: 3)");
    println("arr2[3] = ", arr2[3], " (expected: 4)");
    println("arr2[4] = ", arr2[4], " (expected: 5)");
    
    return 0;
}
