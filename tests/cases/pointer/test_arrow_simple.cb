// 簡単なアロー演算子テスト

struct Point {
    int x;
    int y;
};

int main() {
    Point p = {x: 10, y: 20};
    Point* ptr = &p;
    
    // Test 1: 読み取り
    println("ptr->x = ");
    println(ptr->x);
    
    // Test 2: 代入
    ptr->x = 30;
    println("After ptr->x = 30:");
    println(p.x);
    
    return 0;
}
