// Test 1: Basic struct variable assignment to array element
// このテストは、構造体変数を配列要素に代入する基本的なパターンを検証します

struct Task {
    int task_id;
    int priority;
};

int main() {
    println("=== Test 1: Basic struct variable assignment ===");
    
    Task[10] tasks;
    Task t = {42, 5};
    
    // パターン2: 構造体変数を配列要素に代入
    tasks[0] = t;
    
    // 検証: メンバー変数が正しく設定されているか確認
    println("task_id = ", tasks[0].task_id);
    println("priority = ", tasks[0].priority);
    
    if (tasks[0].task_id == 42 && tasks[0].priority == 5) {
        println("Test 1: Basic struct variable assignment - PASSED");
        return 0;
    } else {
        println("Test 1: FAILED");
        println("Expected: task_id=42, priority=5");
        println("Got: task_id=", tasks[0].task_id, ", priority=", tasks[0].priority);
        return 1;
    }
}
