// Test 5: Function return value assignment to array element
// このテストは、関数の戻り値を配列要素に代入するパターンを検証します

struct Task {
    int task_id;
    int priority;
};

Task create_task(int id, int prio) {
    Task t = {id, prio};
    return t;
}

int main() {
    println("=== Test 1: Function return assignment ===");
    
    Task[10] tasks;
    
    // パターン4: 関数の戻り値を配列要素に代入
    tasks[0] = create_task(999, 88);
    
    // 検証
    println("task_id = ", tasks[0].task_id);
    println("priority = ", tasks[0].priority);
    
    if (tasks[0].task_id == 999 && tasks[0].priority == 88) {
        println("Test 1: Function return assignment - PASSED");
        return 0;
    } else {
        println("Test 1: FAILED");
        return 1;
    }
}
