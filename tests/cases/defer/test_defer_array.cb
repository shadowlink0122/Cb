// defer文と配列のテスト
void main() {
    int[3] arr;
    arr[0] = 1;
    arr[1] = 2;
    arr[2] = 3;
    
    defer {
        println("Array values:");
        println(arr[0]);
        println(arr[1]);
        println(arr[2]);
    }
    
    arr[1] = 10;
    println("Modified array");
}
// 期待される出力:
// Modified array
// Array values:
// 1
// 10
// 3
