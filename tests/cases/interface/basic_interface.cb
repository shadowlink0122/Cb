// 基本的なinterface/implテスト

struct Rectangle {
    int width;
    int height;
};

interface Shape {
    int area();
    int perimeter();
};

impl Shape for Rectangle {
    int area() {
        return self.width * self.height;
    }
    
    int perimeter() {
        return 2 * (self.width + self.height);
    }
};

int main() {
    Rectangle rect = {10, 5};
    Shape shape = rect;
    
    println(shape.area());
    println(shape.perimeter());
    
    return 0;
}
