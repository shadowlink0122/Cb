// interfaceを使わない構造体配列テスト

struct Container {
    int[4] data;
    int count;
};

int main() {
    Container container;
    container.count = 4;
    
    // 直接アクセスでエラーが起きるか確認
    container.data[0] = 1;
    container.data[1] = 2;
    container.data[2] = 3;
    container.data[3] = 4;
    
    println(container.data[0]);
    println(container.data[1]);
    println(container.data[2]);
    println(container.data[3]);
    
    return 0;
}
