// N次元配列の制限を調査するテスト

int main() {
    // 1次元配列テスト
    println("1次元配列テスト:");
    int[3] arr1d = [1, 2, 3];
    println(arr1d[0]);
    println(arr1d[1]);
    println(arr1d[2]);
    
    // 2次元配列テスト
    println("2次元配列テスト:");
    int[2][2] arr2d = [
        [1, 2],
        [3, 4]
    ];
    println(arr2d[0][0]);
    println(arr2d[1][1]);
    
    // 3次元配列テスト
    println("3次元配列テスト:");
    int[2][2][2] arr3d = [
        [[1, 2], [3, 4]],
        [[5, 6], [7, 8]]
    ];
    println(arr3d[0][0][0]);
    println(arr3d[1][1][1]);
    
    return 0;
}
