// 1次元配列の構造体メンバーアクセステスト

struct SimpleArray {
    int[4] data;
    int size;
};

int main() {
    SimpleArray arr;
    arr.size = 4;
    
    // 1次元配列への代入
    arr.data[0] = 1;
    arr.data[1] = 2;
    arr.data[2] = 3;
    arr.data[3] = 4;
    
    // 読み取り
    println(arr.data[0]);
    println(arr.data[1]);
    println(arr.data[2]);
    println(arr.data[3]);
    
    return 0;
}
