// 構造体メンバーとしての多次元配列の問題を調査

struct SimpleMatrix {
    int[2][2] data;
    int size;
};

int main() {
    // 個別に要素を設定してみる
    SimpleMatrix matrix;
    matrix.size = 4;
    
    // 各要素に個別にアクセスを試す
    matrix.data[0][0] = 1;
    matrix.data[0][1] = 2;
    matrix.data[1][0] = 3;
    matrix.data[1][1] = 4;
    
    // 読み取りを試す
    println(matrix.data[0][0]);
    println(matrix.data[0][1]);
    println(matrix.data[1][0]);
    println(matrix.data[1][1]);
    
    return 0;
}
