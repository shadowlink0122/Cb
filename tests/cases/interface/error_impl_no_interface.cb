// implが定義されているが、interfaceがない場合のエラーテスト

struct Calculator {
    int value;
};

// interfaceが定義されていないのにimplを定義
impl UndefinedInterface for Calculator {
    int add(int a, int b) {
        return a + b;
    }
    
    int multiply(int a, int b) {
        return a * b;
    }
};

int main() {
    Calculator calc;
    calc.value = 10;
    
    // これはエラーになるべき（interfaceがないのでメソッドは呼び出せない）
    int result = calc.add(5, 3);
    println(result);
    
    return 0;
}
