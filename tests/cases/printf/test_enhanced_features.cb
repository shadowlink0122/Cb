// 拡張されたprint機能の完全なテスト
int main() {
    print("=== 基本的なprintf機能 ===");
    print("数値: %d", 42);
    print("文字列: %s", "Hello");
    
    print("=== ゼロパディング ===");
    print("ゼロパディング: %02d", 5);
    print("ゼロパディング: %03d", 42);
    print("ゼロパディング: %05d", 123);
    
    print("=== 途中に文字列リテラルがある場合 ===");
    int a = 100;
    int b = 7;
    print(a, "%02d", b);        // "100 07"
    print("値1:", a, "値2:", b); // "値1: 100 値2: 7"
    
    print("=== 複数のフォーマット指定子 ===");
    print("前置き", "%d", 999, "後置き", "%03d", 5); // "前置き 999 後置き 005"
    
    print("=== 余分な引数 ===");
    print("%d", 10, 20, 30, 40); // "10 20 30 40"
    
    return 0;
}
