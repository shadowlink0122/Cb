// 拡張されたprint機能の完全なテスト
int main() {
    println("=== 基本的なprintf機能 ===");
    println("数値: %d", 42);
    println("文字列: %s", "Hello");
    
    println("=== ゼロパディング ===");
    println("ゼロパディング: %02d", 5);
    println("ゼロパディング: %03d", 42);
    println("ゼロパディング: %05d", 123);
    
    println("=== 途中に文字列リテラルがある場合 ===");
    int a = 100;
    int b = 7;
    println(a, "%02d", b);        // "100 07"
    println("値1:", a, "値2:", b); // "値1: 100 値2: 7"
    
    println("=== 複数のフォーマット指定子 ===");
    println("前置き", "%d", 999, "後置き", "%03d", 5); // "前置き 999 後置き 005"
    
    println("=== 余分な引数 ===");
    println("%d", 10, 20, 30, 40); // "10 20 30 40"
    
    return 0;
}
