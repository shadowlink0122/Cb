int main() {
    // 無効なフォーマット指定子
    println("Invalid: %0xd", 123);
    println("Invalid: %0.5d", 42);
    println("Invalid: %0-3d", 5);
    
    // 引数不足
    println("Missing arg: %05d");
    
    // 文字列に対するゼロパディング（無効）
    println("String zero: %05s", "test");
    
    // 不正な桁数指定
    println("Bad width: %0d", 42);
    println("No width: %0", 42);
    
    return 0;
}
