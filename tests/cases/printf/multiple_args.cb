// printf機能の複数引数テスト
int main() {
    // 複数の値を含むフォーマット
    string tanaka = "田中";
    println("名前: {tanaka}, 年齢: 25");
    println("結果: {10 + 20 + 30}");
    println("A B C");
    
    // 混合型の引数
    char x = 'X';
    int num = 100;
    string test = "test";
    println("文字: {x}, 数値: {num}, 文字列: {test}");
    
    // 変数を使用
    string name = "鈴木";
    int age = 30;
    char grade = 'A';
    println("生徒情報: {name} ({age}歳) - 成績: {grade}");
    
    return 0;
}
