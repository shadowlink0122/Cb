// フォーマット指定子と制御文字のテスト
int main() {
    // %02d のようなゼロパディング
    println("%02d", 5);      // "05" と表示されてほしい
    println("%03d", 42);     // "042" と表示されてほしい
    println("%05d", 123);    // "00123" と表示されてほしい
    
    // \n 制御文字
    println("行1\n行2");     // 2行に分かれて表示されてほしい
    println("タブ\t区切り");  // タブ区切りで表示されてほしい
    
    // 複合的な例
    println("値: %02d\n", 7); // "値: 07" + 改行
    
    return 0;
}
