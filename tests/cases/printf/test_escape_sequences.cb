// エスケープ文字の詳細テスト
int main() {
    // 改行文字のテスト
    print("行1\\n行2");
    print("Hello\\nWorld");
    
    // タブ文字のテスト  
    print("列1\\t列2\\t列3");
    
    // クオートのテスト
    print("彼は\\\"こんにちは\\\"と言った");
    print("シングルクオート: \\'test\\'");
    
    // バックスラッシュのテスト
    print("パス: C:\\\\Users\\\\test");
    
    // printf形式でのエスケープ文字
    print("値: %d\\n次の値: %d", 10, 20);
    
    return 0;
}
