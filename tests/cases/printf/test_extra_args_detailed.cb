// 引数が多い場合のテスト - 余分な引数も表示
int main() {
    // printf形式で余分な引数がある場合
    println("値: %d", 100, 200, 300);    // "値: 100 200 300"
    println("名前: %s", "田中", "次郎", "三郎");  // "名前: 田中 次郎 三郎"
    println("%d + %d = %d", 5, 3, 8, 999); // "5 + 3 = 8 999"
    
    // フォーマット指定子がない場合
    println("Hello", "World", "!");      // "Hello World !"
    
    // 混合型の余分な引数
    string name = "佐藤";
    int age = 25;
    println("情報: %s", name, age, "追加情報"); // "情報: 佐藤 25 追加情報"
    
    return 0;
}
