// printf機能の基本的なフォーマット指定子のテスト
int main() {
    // %d - 整数
    println("%d", 42);
    println("数値: %d", 123);
    
    // %s - 文字列
    println("%s", "Hello");
    println("メッセージ: %s", "World");
    
    // %c - 文字
    println("%c", 'A');
    println("文字: %c", 'Z');
    
    // %lld - long long (64bit整数)
    long value = 1234567890;
    println("%lld", value);
    println("長整数: %lld", value);
    
    // %% - パーセント記号のエスケープ（単純なケース）
    println("100% test");
    println("進捗: %d%", 50);
    
    return 0;
}
