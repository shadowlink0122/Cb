// Expression embedding の基本的なテスト（旧printf機能の代替）
int main() {
    // 整数の埋め込み
    println("42");
    println("数値: 123");
    
    // 文字列の埋め込み
    println("Hello");
    println("メッセージ: World");
    
    // 文字の埋め込み
    println("A");
    println("文字: Z");
    
    // long long (64bit整数)
    long value = 1234567890;
    println("{value}");
    println("長整数: {value}");
    
    // パーセント記号（そのまま出力）
    println("100% test");
    println("進捗: 50%");
    
    return 0;
}
