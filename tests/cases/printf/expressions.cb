// printf機能の式と計算結果テスト
int main() {
    int a = 10;
    int b = 5;
    int sum = 15;
    int diff = 5;
    int mult = 50;
    int div = 2;
    int complex = 30;
    
    // 計算結果の出力
    println("{a} + {b} = {sum}");
    println("{a} - {b} = {diff}");
    println("{a} * {b} = {mult}");
    println("{a} / {b} = {div}");
    
    // 複雑な式
    println("(({a} + {b}) * 2) = {complex}");
    
    // 配列要素
    int val1 = 1;
    int val2 = 2; 
    int val3 = 3;
    println("配列: [{val1}, {val2}, {val3}]");
    
    // 文字列変数のテスト
    string word1 = "Hello";
    string word2 = "World";
    println("文字列配列: {word1} {word2}");
    
    return 0;
}
