// const変数の読み取り正常動作テスト

int main() {
    println("Testing const variables read access:");
    
    // const数値変数
    const int num = 42;
    const tiny small = 5;
    const short medium = 1000;
    const long big = 1000000;
    
    // const string変数
    const string greeting = "Hello";
    const string name = "World";
    
    // 読み取りは正常に動作するはず
    println("Int value: " + num);
    println("Tiny value: " + small);
    println("Short value: " + medium);
    println("Long value: " + big);
    println("String1: " + greeting);
    println("String2: " + name);
    
    // 式で使用
    int sum = num + small;
    println("Sum: " + sum);
    
    // 文字列結合
    string combined = greeting + " " + name;
    println("Combined: " + combined);
    
    println("All const variable reads completed successfully");
    return 0;
}
