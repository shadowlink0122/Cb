// 基本的なリテラル値ユニオンテスト

// 数値リテラルユニオン
typedef StatusCode = 200 | 404 | 500;

// Boolean リテラルユニオン  
typedef Flag = true | false;

// 文字リテラルユニオン
typedef Grade = 'A' | 'B' | 'C' | 'D' | 'F';

// 文字列リテラルユニオン
typedef LogLevel = "DEBUG" | "INFO" | "WARN" | "ERROR";

int main() {
    println("=== 基本リテラル値ユニオンテスト ===");
    
    // 数値リテラルテスト
    StatusCode ok = 200;
    StatusCode notFound = 404;
    StatusCode serverError = 500;
    println("HTTP Status Codes:");
    println("  OK: {ok}");
    println("  Not Found: {notFound}");
    println("  Server Error: {serverError}");
    
    // Boolean リテラルテスト
    Flag enabled = true;
    Flag disabled = false;
    println("Boolean Flags:");
    println("  Enabled: {enabled}");
    println("  Disabled: {disabled}");
    
    // 文字リテラルテスト
    Grade excellent = 'A';
    Grade good = 'B';
    Grade average = 'C';
    Grade poor = 'D';
    Grade fail = 'F';
    println("Grade Letters:");
    println("  Excellent: {excellent}");
    println("  Good: {good}");
    println("  Average: {average}");
    println("  Poor: {poor}");
    println("  Fail: {fail}");
    
    // 文字列リテラルテスト
    LogLevel debug = "DEBUG";
    LogLevel info = "INFO";
    LogLevel warn = "WARN";
    LogLevel error = "ERROR";
    println("Log Levels:");
    println("  Debug: {debug}");
    println("  Info: {info}");
    println("  Warning: {warn}");
    println("  Error: {error}");
    
    println("=== 基本リテラル値ユニオンテスト完了 ===");
    return 0;
}
