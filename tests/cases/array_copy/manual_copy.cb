// 配列コピー（手動）テスト
int main() {
    // 元の配列
    int[3] src = [10, 20, 30];
    
    // コピー先の配列
    int[3] dest;
    
    // 手動コピー
    for (int i = 0; i < 3; i = i + 1) {
        dest[i] = src[i];
    }
    
    // コピー結果の確認
    print(dest[0]);
    print(dest[1]);
    print(dest[2]);
    
    // 元の配列を変更
    src[0] = 999;
    
    // コピー先は変更されないことを確認
    print(dest[0]); // should still be 10
    
    return 0;
}
