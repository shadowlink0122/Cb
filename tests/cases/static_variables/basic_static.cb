// 基本的なstatic変数の動作テスト
int counter(){
    static int count = 0;
    count++;
    return count;
}

int main(){
    // static変数は初期化されて、値が保持される
    println(counter()); // 1
    println(counter()); // 2
    println(counter()); // 3
    println(counter()); // 4
    println(counter()); // 5
    return 0;
}
