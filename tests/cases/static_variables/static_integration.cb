// Integration test for static variables
int counter(){
    static int count = 0;
    count++;
    return count;
}

int func1(){
    static int value = 10;
    value += 5;
    return value;
}

int func2(){
    static int value = 100;
    value -= 10;
    return value;
}

void test_static_const(){
    static const int MAX = 50;
    static int current = 0;
    
    current += 12;
    if(current > MAX){
        current = MAX;
    }
    
    println(current);
}

int main(){
    // Basic static variable
    println(counter()); // 1
    println(counter()); // 2
    println(counter()); // 3
    
    // Multiple functions with same variable name
    println(func1()); // 15
    println(func2()); // 90
    println(func1()); // 20
    println(func2()); // 80
    
    // Static const combination
    test_static_const(); // 12
    test_static_const(); // 24
    test_static_const(); // 36
    test_static_const(); // 48
    test_static_const(); // 50
    test_static_const(); // 50
    
    return 0;
}
