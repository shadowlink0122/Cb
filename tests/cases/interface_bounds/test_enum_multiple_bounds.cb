// Test: Multiple interface bounds on generic enums
// Expected: Parse successfully

interface Clone {
    void clone();
}

interface Debug {
    void debug();
}

interface Allocator {
    void* allocate(int size);
    void deallocate(void* ptr);
}

// Enum with multiple bounds
enum Result<T: Clone + Debug, E> {
    Ok(T),
    Err(E)
};

// Enum with three bounds on one parameter
enum Container<T: Allocator + Clone + Debug> {
    Some(T),
    None
};

int main() {
    println("=== Generic Enum Multiple Bounds Test ===");
    
    // Test 1: Enum with single type parameter and multiple bounds
    println("Test 1: Result<T: Clone + Debug, E> - PASSED");
    
    // Test 2: Enum with type parameter having three bounds
    println("Test 2: Container<T: Allocator + Clone + Debug> - PASSED");
    
    println("=== All Tests Passed ===");
    return 0;
}
