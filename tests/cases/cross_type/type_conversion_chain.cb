// Test: Type conversion chain
// Tests complex type conversions across different types

func int main() {
    // tiny -> short -> int -> long chain
    tiny t = 100;
    short s = t;
    int i = s;
    long l = i;
    
    println(l);  // Should be 100
    
    // Reverse chain (explicit casts in real code)
    long l2 = 42;
    int i2 = l2;
    short s2 = i2;
    tiny t2 = s2;
    
    println(t2);  // Should be 42
    
    // Float to double and back
    float f = 3.14;
    double d = f;
    float f2 = d;
    
    println(f2);
    
    return 0;
}
