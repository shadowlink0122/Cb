// Test: Unsigned type arithmetic
// Tests unsigned type operations and overflow behavior

func int main() {
    unsigned tiny ut = 200;
    unsigned short us = 50000;
    unsigned int ui = 3000000000;
    unsigned long ul = 10000000000;
    
    // Addition
    unsigned tiny ut2 = ut + 50;  // 250
    println(ut2);
    
    // Subtraction (should not go negative)
    unsigned short us2 = us - 1000;
    println(us2);
    
    // Multiplication
    unsigned int ui2 = ui + 100000;
    println(ui2);
    
    // Comparison
    if (ut < us) {
        println("unsigned comparison works");
    }
    
    return 0;
}
