
int main() {
  // 型間演算・代入テスト
  tiny t = 10;
  short s = 20;
  int i = 30;
  long l = 40;
  // 型違いの四則演算
  print(t + s);
  print(s + i);
  print(i + l);
  print(l + t);
  print(t * l);
  // 型違いの代入
  short s2 = t;
  int i2 = s;
  long l2 = i;
  print(s2); print(i2); print(l2);
  // 境界値を型違いで演算
  print(t + 127);
  print(s + 32767);
  print(i + 2147483647);
  print(l + 9223372036854775807);
}
