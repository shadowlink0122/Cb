// Double型の基本テスト
int main() {
    double a = 3.141592653589793;
    double b = 2.718281828459045;
    
    // 四則演算
    println(a + b);  // 5.859874482048838
    println(a - b);  // 0.423310825130748
    println(a * b);  // 8.539734222673566
    println(a / b);  // 1.155727349790921
    
    // 比較演算
    if (a > b) {
        println("a > b: true");
    }
    
    // 高精度計算
    double c = 1.0 / 3.0;
    println(c);  // 0.333333333333...
    
    double d = (1.0 / 3.0) * 3.0;
    println(d);  // 0.999999999999...
    
    // 整数との混合演算
    double e = 10.5 + 5;
    println(e);  // 15.5
    
    double f = 10 / 3.0;
    println(f);  // 3.333333...
    
    return 0;
}
