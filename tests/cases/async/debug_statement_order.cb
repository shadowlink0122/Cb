// Debug test: Check statement execution order

async Future<int> debug_task() {
    println("[1] First statement");
    int x = 1;
    println("[2] After variable declaration");
    if (x > 0) {
        println("[3] Inside if");
    }
    println("[4] After if");
    for (int i = 0; i < 2; i++) {
        println("[5] Loop i={i}");
    }
    println("[6] After loop");
    return 42;
}

void main() {
    Future<int> f = debug_task();
    int r = await f;
    println("[Main] Result={r}");
}
