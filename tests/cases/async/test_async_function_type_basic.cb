// Test: Async関数型の基本機能
// async関数を引数として受け取る

async int compute() {
    return 42;
}

void executor(async int* callback) {
    int result = await callback();
    println("Result: {result}");
}

void main() {
    println("=== Async Function Type Basic Test ===");
    executor(&compute);
    println("=== Test Complete ===");
}
