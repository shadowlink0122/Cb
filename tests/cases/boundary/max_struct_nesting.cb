// Test: Struct with maximum nesting
// Tests deeply nested struct member access

struct Level3 {
    int value;
};

struct Level2 {
    Level3 level3;
    int data2;
};

struct Level1 {
    Level2 level2;
    int data1;
};

struct Root {
    Level1 level1;
    int root_data;
};

int main() {
    Root r;
    r.root_data = 100;
    r.level1.data1 = 200;
    r.level1.level2.data2 = 300;
    r.level1.level2.level3.value = 400;
    
    int sum = r.root_data + r.level1.data1 + 
              r.level1.level2.data2 + r.level1.level2.level3.value;
    
    println(sum);  // Should be 1000
    return sum;
}
