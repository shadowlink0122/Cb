// Test: Pointer arithmetic edge cases
// Tests boundary conditions for pointer arithmetic

int main() {
    int[5] arr = [10, 20, 30, 40, 50];
    int* ptr = &arr[0];
    
    // Valid: Move to last element
    ptr = ptr + 4;
    int val1 = *ptr;  // Should be 50
    
    // Valid: Move back to first
    ptr = ptr - 4;
    int val2 = *ptr;  // Should be 10
    
    // Test pointer comparison
    int* ptr2 = &arr[2];
    if (ptr < ptr2) {
        println("Pointer comparison works");
    }
    
    return val1 + val2;  // Should be 60
}
