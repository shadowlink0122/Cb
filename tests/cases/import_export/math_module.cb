// import/export テスト用モジュール
export const int MATH_CONSTANT = 42;
export const string LIBRARY_NAME = "MathLib";

export int multiply(int a, int b) {
    return a * b;
}

export int divide(int a, int b) {
    if (b == 0) {
        println("Error: Division by zero!");
        return 0;
    }
    return a / b;
}
