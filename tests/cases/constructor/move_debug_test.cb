// v0.10.0 ムーブセマンティクス - デバッグテスト

struct Point {
    int x;
    int y;
};

impl Point {
    self(int px, int py) {
        self.x = px;
        self.y = py;
        println("[DEBUG] Default constructor: (", px, ", ", py, ")");
    }
    
    // コピーコンストラクタ
    self(const Point& other) {
        self.x = other.x;
        self.y = other.y;
        println("[DEBUG] Copy constructor called");
        println("[DEBUG]   Source: (", other.x, ", ", other.y, ")");
    }
    
    // ムーブコンストラクタ
    self(Point&& other) {
        println("[DEBUG] Move constructor called");
        println("[DEBUG]   Source before: (", other.x, ", ", other.y, ")");
        
        self.x = other.x;
        self.y = other.y;
        
        // 元のオブジェクトを無効化
        other.x = 0;
        other.y = 0;
        
        println("[DEBUG]   Source after: (", other.x, ", ", other.y, ")");
        println("[DEBUG]   Dest: (", self.x, ", ", self.y, ")");
    }
    
    ~self() {
        println("[DEBUG] Destructor: (", self.x, ", ", self.y, ")");
    }
}

void main() {
    println("=== Move Semantics Debug Test ===");
    
    println("\n1. Creating p1:");
    Point p1(10, 20);
    println("   p1: (", p1.x, ", ", p1.y, ")");
    
    println("\n2. Move p1 to p2:");
    Point p2 = move(p1);
    println("   p2: (", p2.x, ", ", p2.y, ")");
    println("   p1: (", p1.x, ", ", p1.y, ")");
    
    println("\n3. End of main (destructors will be called)");
}
