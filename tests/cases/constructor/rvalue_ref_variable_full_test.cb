// v0.10.0 T&&変数（右辺値参照型変数）のテスト

struct Point {
    int x;
    int y;
};

impl Point {
    self(int px, int py) {
        self.x = px;
        self.y = py;
        println("[DEBUG] Constructor: (", px, ", ", py, ")");
    }
    
    self(const Point& other) {
        self.x = other.x;
        self.y = other.y;
        println("[DEBUG] Copy constructor");
    }
    
    self(Point&& other) {
        self.x = other.x;
        self.y = other.y;
        other.x = 0;
        other.y = 0;
        println("[DEBUG] Move constructor");
    }
    
    ~self() {
        println("[DEBUG] Destructor: (", self.x, ", ", self.y, ")");
    }
}

void main() {
    println("=== T&& Variable Test ===");
    
    println("\n1. 右辺値参照型変数の作成:");
    Point p1(10, 20);
    Point&& ref = move(p1);
    println("   ref: (", ref.x, ", ", ref.y, ")");
    println("   p1: (", p1.x, ", ", p1.y, ")");
    
    println("\n2. 右辺値参照経由で変更:");
    ref.x = 100;
    println("   ref: (", ref.x, ", ", ref.y, ")");
    println("   p1: (", p1.x, ", ", p1.y, ")");
    
    println("\n3. End of main");
}
