int& forward_ref(int& x) {
    println("In forward_ref, x =");
    println(x);
    return x;
}

int main() {
    int local = 50;
    println("Before call, local =");
    println(local);
    int& ref = forward_ref(local);
    println("After call, ref =");
    println(ref);
    ref = 75;
    println("After modification, local =");
    println(local);
    return 0;
}
