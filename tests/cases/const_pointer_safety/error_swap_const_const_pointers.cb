// const T* const のswap不可テスト
// 2つのconst*constポインタをswapできないことを確認

int main() {
    int a = 100, b = 200;
    const int* const ptr1 = &a;
    const int* const ptr2 = &b;
    
    println("Attempting to swap const * const pointers...");
    
    // swapの試行（どれもエラーになるべき）
    const int* const temp = ptr1;  // これは新しい変数なのでOK
    ptr1 = ptr2;  // Error: Cannot reassign const pointer
    ptr2 = temp;  // Error: Cannot reassign const pointer
    
    return 0;
}
