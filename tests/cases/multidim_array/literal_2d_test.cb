
int main() {
    // 2次元配列リテラル
    int[2][3] matrix = [
        [1, 2, 3],
        [4, 5, 6]
    ];
    
    println("2D literal array values:");
    for (int i = 0; i < 2; i++) {
        for (int j = 0; j < 3; j++) {
            println("matrix[%d][%d] = %d", i, j, matrix[i][j]);
        }
    }
    
    // 値の確認
    if (matrix[0][0] != 1) return 1;
    if (matrix[0][1] != 2) return 1;
    if (matrix[0][2] != 3) return 1;
    if (matrix[1][0] != 4) return 1;
    if (matrix[1][1] != 5) return 1;
    if (matrix[1][2] != 6) return 1;
    
    println("All 2D literal tests passed");
    return 0;
}
    