// 3次元配列のテスト
int main() {
    // 3次元配列の宣言と手動初期化
    int[2][2][2] cube;
    
    // 値の設定
    cube[0][0][0] = 100;
    cube[0][0][1] = 101;
    cube[0][1][0] = 102;
    cube[0][1][1] = 103;
    cube[1][0][0] = 104;
    cube[1][0][1] = 105;
    cube[1][1][0] = 106;
    cube[1][1][1] = 107;
    
    println("3D cube data:");
    for (int i = 0; i < 2; i++) {
        for (int j = 0; j < 2; j++) {
            for (int k = 0; k < 2; k++) {
                println("cube[{i}][{j}][{k}] = {cube[i][j][k]}");
            }
        }
    }
    
    // 値の確認
    if (cube[0][0][0] != 100) return 1;
    if (cube[0][0][1] != 101) return 1;
    if (cube[0][1][0] != 102) return 1;
    if (cube[0][1][1] != 103) return 1;
    if (cube[1][0][0] != 104) return 1;
    if (cube[1][0][1] != 105) return 1;
    if (cube[1][1][0] != 106) return 1;
    if (cube[1][1][1] != 107) return 1;
    
    println("All cube tests passed");
    return 0;
}
