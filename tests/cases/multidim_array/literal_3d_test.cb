
int main() {
    // 3次元配列リテラル
    int[2][2][2] cube = [
        [
            [100, 101],
            [102, 103]
        ],
        [
            [104, 105],
            [106, 107]
        ]
    ];
    
    println("3D array values:");
    for (int i = 0; i < 2; i++) {
        for (int j = 0; j < 2; j++) {
            for (int k = 0; k < 2; k++) {
                println("cube[%d][%d][%d] = %d", i, j, k, cube[i][j][k]);
            }
        }
    }
    
    // 値の確認
    if (cube[0][0][0] != 100) return 1;
    if (cube[0][0][1] != 101) return 1;
    if (cube[0][1][0] != 102) return 1;
    if (cube[0][1][1] != 103) return 1;
    if (cube[1][0][0] != 104) return 1;
    if (cube[1][0][1] != 105) return 1;
    if (cube[1][1][0] != 106) return 1;
    if (cube[1][1][1] != 107) return 1;
    
    println("All 3D tests passed");
    return 0;
}
    