// 多次元配列リテラル初期化のテスト
int main() {
    // 2次元配列リテラル
    int[2][3] matrix = [
        [1, 2, 3],
        [4, 5, 6]
    ];
    
    println("Literal initialized matrix:");
    for (int i = 0; i < 2; i++) {
        for (int j = 0; j < 3; j++) {
            println("matrix[{i}][{j}] = {matrix[i][j]}");
        }
    }
    
    // 期待値の確認
    if (matrix[0][0] != 1) return 1;
    if (matrix[0][1] != 2) return 1;
    if (matrix[0][2] != 3) return 1;
    if (matrix[1][0] != 4) return 1;
    if (matrix[1][1] != 5) return 1;
    if (matrix[1][2] != 6) return 1;
    
    println("All literal init tests passed");
    return 0;
}
