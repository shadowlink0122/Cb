// 文字列戻り値の三項演算子テスト

string get_positive() {
    return "positive";
}

string get_negative() {
    return "negative";  
}

string get_zero() {
    return "zero";
}

string simple_classification(int value) {
    return (value > 0) ? "positive" : "negative";
}

int main() {
    println("=== String Return Value Ternary Test ===");
    
    // 1. 文字列リテラルによる三項演算子
    bool is_positive = true;
    string result1 = is_positive ? "pos" : "neg";
    println("Literal ternary: %s", result1);
    
    // 2. 関数呼び出しによる三項演算子  
    int value = 10;
    string result2 = (value > 0) ? get_positive() : get_negative();
    println("Function ternary: %s", result2);
    
    // 3. 関数内での三項演算子
    string result3 = simple_classification(15);
    println("Function with ternary: %s", result3);
    
    string result4 = simple_classification(-5);
    println("Function with ternary 2: %s", result4);
    
    // 4. 複雑な条件での文字列戻り値
    int test_value = 0;
    string result5 = (test_value > 0) ? get_positive() : 
                     (test_value < 0) ? get_negative() : get_zero();
    println("Complex ternary: %s", result5);
    
    println("=== String Return Value Test Complete ===");
    return 0;
}
