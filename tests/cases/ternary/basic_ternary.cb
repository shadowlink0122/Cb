// 三項演算子の基本テスト
main() {
    println("Ternary operator test:");
    int a, b, result;
    
    // 基本的な三項演算子
    a = 10;
    b = 20;
    result = (a > b) ? a : b;  // aとbの最大値
    print("max(10, 20): "); println(result);  // 20
    
    result = (a < b) ? a : b;  // aとbの最小値
    print("min(10, 20): "); println(result);  // 10
    
    // 条件判定
    int score = 85;
    result = (score >= 60) ? 1 : 0;  // 合格判定
    print("score 85 pass: "); println(result);  // 1
    
    score = 45;
    result = (score >= 60) ? 1 : 0;  // 不合格判定
    print("score 45 pass: "); println(result);  // 0
    
    // ネストした三項演算子
    int x = 5;
    result = (x > 10) ? 1 : (x > 0) ? 0 : -1;
    print("nested ternary x=5: "); println(result);  // 0
    
    x = 15;
    result = (x > 10) ? 1 : (x > 0) ? 0 : -1;
    print("nested ternary x=15: "); println(result);  // 1
    
    x = -5;
    result = (x > 10) ? 1 : (x > 0) ? 0 : -1;
    print("nested ternary x=-5: "); println(result);  // -1
    
    println("Ternary operator test passed");
}
