struct Inner {
    int value;
    string name;
};

struct Middle {
    Inner data;
};

struct Outer {
    Middle mid;
};

int main() {
    Outer obj;
    obj = {mid: {data: {value: 10, name: "initial"}}};
    
    println("Initial:");
    println("  obj.mid.data.value:", obj.mid.data.value);
    println("  obj.mid.data.name:", obj.mid.data.name);
    
    obj.mid.data.value = 100;
    obj.mid.data.name = "updated";
    
    println("After:");
    println("  obj.mid.data.value:", obj.mid.data.value);
    println("  obj.mid.data.name:", obj.mid.data.name);
    
    if (obj.mid.data.value == 100 && obj.mid.data.name == "updated") {
        println("SUCCESS!");
        return 0;
    } else {
        println("FAILED!");
        return 1;
    }
}
