// 最小限のネストメンバテスト

struct Point {
    int x;
    int y;
};

struct Box {
    Point topLeft;
};

int main() {
    Box box1;
    
    println("=== Test: Nested member write ===");
    box1.topLeft.x = 42;
    println("Set box1.topLeft.x = 42");
    
    println("");
    println("=== Test: Nested member read ===");
    int value = box1.topLeft.x;
    println("Read into variable:");
    println(value);
    
    println("");
    println("=== Test: Direct print ===");
    println(box1.topLeft.x);
    
    return 0;
}
