// ネストした構造体の基本テスト

struct Point {
    int x;
    int y;
};

struct Box {
    Point topLeft;
};

int main() {
    Box box1;
    
    println("Test 1: Direct member access");
    box1.topLeft.x = 42;
    println("Set box1.topLeft.x = 42");
    println("Reading box1.topLeft.x:");
    println(box1.topLeft.x);  // Should print 42
    
    println("");
    println("Test 2: Both x and y");
    box1.topLeft.y = 100;
    println("Set box1.topLeft.y = 100");
    println("x:");
    println(box1.topLeft.x);  // Should print 42
    println("y:");
    println(box1.topLeft.y);  // Should print 100
    
    return 0;
}
