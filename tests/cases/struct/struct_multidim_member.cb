// 構造体メンバの多次元配列テスト
struct Matrix3x3 {
    string name;
    int[3][3] data;  // 3x3行列
    int rows;
    int cols;
};

int main() {
    Matrix3x3 m;
    m.name = "Test Matrix";
    m.rows = 3;
    m.cols = 3;
    
    // 多次元配列の初期化
    m.data = [
        [1, 2, 3],
        [4, 5, 6],
        [7, 8, 9]
    ];
    
    // 個別要素のアクセス
    m.data[0][0] = 10;
    m.data[1][1] = 50;
    m.data[2][2] = 90;
    
    // 値の確認
    println("Matrix:", m.name);
    println("Size: %dx%d", m.rows, m.cols);
    println("Element [0][0]:", m.data[0][0]);
    println("Element [1][1]:", m.data[1][1]);
    println("Element [2][2]:", m.data[2][2]);
    
    // 行の出力
    println("Row 0: [%d, %d, %d]", m.data[0][0], m.data[0][1], m.data[0][2]);
    println("Row 1: [%d, %d, %d]", m.data[1][0], m.data[1][1], m.data[1][2]);
    println("Row 2: [%d, %d, %d]", m.data[2][0], m.data[2][1], m.data[2][2]);
    
    return 0;
}
