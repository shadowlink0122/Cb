// 最小限のconst構造体ネストテスト

struct Inner {
    int value;
};

struct Outer {
    Inner inner;
};

int main() {
    println("=== Simple Const Nested Test ===");
    
    // Test: const構造体のネストしたメンバー
    Inner inner_data = {value: 42};
    const Outer o = {inner: inner_data};
    
    println("o.inner.value = %d", o.inner.value);
    
    // ネストしたメンバーの変更を試みる（エラーになるべき）
    println("Attempting to modify nested const member...");
    o.inner.value = 100;
    
    println("ERROR: This should not be reached!");
    return 1;
}
