struct Inner {
    int value;
};

struct Middle {
    Inner data;
};

struct Outer {
    Middle mid;
    int id;
};

int main() {
    Outer obj;
    obj = {mid: {data: {value: 10}}, id: 1};
    
    println("Initial:");
    println("  obj.mid.data.value:", obj.mid.data.value);
    println("  obj.id:", obj.id);
    
    obj.mid.data.value = 100;
    println("After obj.mid.data.value = 100:");
    println("  obj.mid.data.value:", obj.mid.data.value);
    println("  obj.id:", obj.id);
    
    obj.id = 2;
    println("After obj.id = 2:");
    println("  obj.mid.data.value:", obj.mid.data.value);
    println("  obj.id:", obj.id);
    
    return 0;
}
