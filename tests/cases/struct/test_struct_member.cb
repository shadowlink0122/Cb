// 構造体の構造体メンバのテスト

struct Private {
    int secret;
};

struct Inner {
    int value;
    Private pr;
};

struct Outer {
    Inner inner;
    int count;
};

int main() {
    println("=== Struct Member Test ===");
    
    Private pr = {secret: 42};
    Inner i = {value: 100, pr: pr};
    Outer o = {inner: i, count: 5};
    
    println("o.inner.value =", o.inner.value);
    println("o.inner.pr.secret =", o.inner.pr.secret);
    println("o.count =", o.count);
    o = {inner: {value: 200, pr: {secret: 84}}, count: 10};
    println("o.inner.value =", o.inner.value);
    println("o.inner.pr.secret =", o.inner.pr.secret);
    println("o.count =", o.count);
    println("=== Test passed ===");
    
    return 0;
}
