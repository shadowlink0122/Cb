// Struct private field success scenario

struct SecretBox {
    private int secret;
};

interface SecretBoxOps {
    void runDemo();
};

impl SecretBoxOps for SecretBox {
    void runDemo() {
        self.secret = 42;
        println("box secret after init = {self.secret}");

        SecretBox other = {};
        other.secret = 100;
        self.secret = other.secret;
        println("box secret after copy = {self.secret}");
    }
};

int main() {
    println("Private field ok test:");

    SecretBox box = {};
    SecretBoxOps ops = box;
    ops.runDemo();

    println("done");
    return 0;
}
