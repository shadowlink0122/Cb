struct Inner {
    int value;
    string name;
};

struct Middle {
    Inner data;
    int count;
};

struct Outer {
    Middle mid;
    int id;
};

int main() {
    Outer obj;
    obj = {
        mid: {
            data: {value: 10, name: "initial"},
            count: 5
        },
        id: 1
    };
    
    println("Initial:");
    println("  obj.mid.data.value:", obj.mid.data.value);
    println("  obj.mid.count:", obj.mid.count);
    println("  obj.id:", obj.id);
    
    obj.mid.data.value = 100;
    obj.mid.count = 50;
    
    println("After (no obj.id assignment):");
    println("  obj.mid.data.value:", obj.mid.data.value);
    println("  obj.mid.count:", obj.mid.count);
    println("  obj.id:", obj.id);
    
    return 0;
}
