// 配列メンバーのテスト（多次元は将来実装予定）
struct Matrix {
    string name;
    int[6] data;  // 2x3の代わりに1次元配列として扱う
    int rows;
    int cols;
};

int main() {
    Matrix m;
    m.name = "Sample Matrix";
    m.rows = 2;
    m.cols = 3;
    
    // 1次元配列として初期化（多次元アクセスは将来実装）
    m.data[0] = 1; m.data[1] = 2; m.data[2] = 3; // Row 0
    m.data[3] = 4; m.data[4] = 5; m.data[5] = 6; // Row 1

    m.data = [1, 2, 3, 4, 5, 6];
    
    println("Matrix: %s (%dx%d)", m.name, m.rows, m.cols);
    println("Row 0: [%d, %d, %d]", m.data[0], m.data[1], m.data[2]);
    println("Row 1: [%d, %d, %d]", m.data[3], m.data[4], m.data[5]);
    
    // 期待される出力:
    // Matrix: Sample Matrix (2x3)
    // Row 0: [1, 2, 3]
    // Row 1: [4, 5, 6]
    return 0;
}
