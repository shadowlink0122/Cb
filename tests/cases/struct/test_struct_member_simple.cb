// 構造体の構造体メンバの簡単なテスト

struct Inner {
    int value;
};

struct Outer {
    Inner inner;
};

int main() {
    Inner i = {value: 100};
    Outer o = {inner: i};
    
    // 直接アクセス
    println("Test 1: o.inner (should be struct)");
    // println(o.inner);  // これは構造体なので(struct)と表示される
    
    println("Test 2: o.inner.value (should be 100)");
    println(o.inner.value);
    
    return 0;
}
