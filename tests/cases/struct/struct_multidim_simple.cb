// 構造体の多次元配列メンバー簡単テスト
struct Matrix2x2 {
    int[2][2] data;
    string name;
};

int main() {
    Matrix2x2 m;
    m.name = "Test";
    
    // 個別要素代入
    m.data[0][0] = 1;
    m.data[0][1] = 2;
    m.data[1][0] = 3;
    m.data[1][1] = 4;
    
    println("Matrix name:", m.name);
    println("Element [0][0]:", m.data[0][0]);
    println("Element [0][1]:", m.data[0][1]);
    println("Element [1][0]:", m.data[1][0]);
    println("Element [1][1]:", m.data[1][1]);
    
    return 0;
}
