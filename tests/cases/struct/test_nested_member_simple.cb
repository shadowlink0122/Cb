// 単純なネストメンバ代入テスト

struct Inner {
    int value;
};

struct Outer {
    Inner inner;
};

int main() {
    println("=== Simple Nested Member Assignment ===");
    
    Outer obj;
    obj = {inner: {value: 10}};
    
    println("Initial: obj.inner.value =", obj.inner.value);
    
    // 直接代入
    obj.inner.value = 999;
    
    println("After assignment: obj.inner.value =", obj.inner.value);
    
    if (obj.inner.value == 999) {
        println("SUCCESS!");
        return 0;
    } else {
        println("FAILED!");
        return 1;
    }
}
