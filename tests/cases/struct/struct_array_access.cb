struct Point {
    int x;
    int y;
};

int main() {
    println("=== Struct Array Access Test ===");
    
    Point[3] points;
    points[0].x = 1;
    points[0].y = 2;
    points[1].x = 3;
    points[1].y = 4;
    points[2].x = 5;
    points[2].y = 6;
    
    for (int i = 0; i < 3; i++) {
        println("Point:");
        println(i);
        println(points[i].x);
        println(points[i].y);
    }
    
    println("=== Test completed ===");
    return 0;
}
