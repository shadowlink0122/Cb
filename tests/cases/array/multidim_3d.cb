int main() {
    // 3D array test
    int a[2][2][2];
    
    a[0][0][0] = 1;
    a[0][0][1] = 2;
    a[0][1][0] = 3;
    a[0][1][1] = 4;
    a[1][0][0] = 5;
    a[1][0][1] = 6;
