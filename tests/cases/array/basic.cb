int main() {
    tiny size = 5;
    int[size] a;
    for(tiny i = 0;i < size;i++){
        a[i] = 10 * (i + 1);
    }
    println(a[0]); // 10
    println(a[4]); // 50
    a[2] = 99;
    println(a[2]); // 99
    int sum = 0;
    for(int i = 0; i < size; i++) {
        sum = sum + a[i];
    }
    println(sum); // 10+20+99+40+50 = 219
}
