int main() {
    tiny size = 5;
    int[size] a;
    for(tiny i = 0;i < size;i++){
        a[i] = 10 * (i + 1);
    }
    
    println("Array basic test:");
    println("a[0] = %d", a[0]); // 10
    println("a[4] = %d", a[4]); // 50
    a[2] = 99;
    println("After modification a[2] = %d", a[2]); // 99
    
    int sum = 0;
    for(int i = 0; i < size; i++) {
        sum = sum + a[i];
    }
    println("sum = %d", sum); // 10+20+99+40+50 = 219
    println("Array basic test passed");
    
    return 0;
}
