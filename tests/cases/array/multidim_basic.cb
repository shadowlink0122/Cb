int main() {
    // Basic 2D array declaration and access
    int a[2][3];
    a[0][0] = 10;
    a[0][1] = 20;
    a[0][2] = 30;
    a[1][0] = 40;
    a[1][1] = 50;
    a[1][2] = 60;
    
    print a[0][0]; // 10
    print a[0][1]; // 20
    print a[0][2]; // 30
    print a[1][0]; // 40
    print a[1][1]; // 50
    print a[1][2]; // 60
    
    return 0;
}