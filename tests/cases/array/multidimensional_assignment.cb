// 2次元配列の代入と参照のテストケース

int main() {
    // 2次元配列の宣言
    int[2][3] matrix;
    
    // 要素への代入
    matrix[0][0] = 10;
    matrix[0][1] = 20;
    matrix[1][2] = 99;
    
    // 要素の参照と出力
    println("matrix[0][0] =", matrix[0][0]);
    println("matrix[0][1] =", matrix[0][1]); 
    println("matrix[1][2] =", matrix[1][2]);
    
    return 0;
}
