// Global array declaration test
int[5] global_array;

int main() {
    global_array[0] = 10;
    global_array[1] = 20;
    global_array[2] = 30;
    
    int temp0 = global_array[0];
    int temp1 = global_array[1];
    int temp2 = global_array[2];
    
    println("global_array[0] = %d", temp0);
    println("global_array[1] = %d", temp1);
    println("global_array[2] = %d", temp2);
    
    return 0;
}
