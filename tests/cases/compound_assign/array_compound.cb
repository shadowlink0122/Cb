// 配列への複合代入演算子のテスト
main() {
    int[5] arr = [10, 20, 30, 40, 50];
    int i;
    
    // 配列要素への複合代入
    arr[0] += 5;
    println(arr[0]);  // 15
    
    arr[1] -= 8;
    println(arr[1]);  // 12
    
    arr[2] *= 2;
    println(arr[2]);  // 60
    
    arr[3] /= 4;
    println(arr[3]);  // 10
    
    arr[4] %= 7;
    println(arr[4]);  // 1
    
    // ループでの使用例
    i = 0;
    while (i < 5) {
        arr[i] += i;  // 各要素にインデックスを加算
        i++;          // i++ の代わりに i = i + 1 を使用
    }
    
    // 結果表示
    i = 0;
    while (i < 5) {
        println(arr[i]);
        i++;    // i++ の代わりに i = i + 1 を使用
    }
    // 15, 13, 62, 13, 5 が出力される
}
