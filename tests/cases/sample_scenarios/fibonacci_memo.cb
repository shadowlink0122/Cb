// Fibonacci with memoization using global array
long[100] memo;

long fib(int n) {
    if (n < 0) return 0;
    if (memo[n] > 0) return memo[n];
    if (n == 0) {
        memo[n] = 0;
    } else if (n == 1) {
        memo[n] = 1;
    } else {
        memo[n] = fib(n - 1) + fib(n - 2);
    }
    return memo[n];
}

int main() {
    // Test fibonacci sequence
    for (int i = 0; i <= 10; i = i + 1) {
        long fib_result = fib(i);
        println("fib(%d) = %d", i, fib_result);
    }
    return 0;
}
