// FizzBuzz implementation test
int main() {
    for (int i = 1; i <= 15; i = i + 1) {
        if ((i % 3 == 0) && (i % 5 == 0)) {
            println("{i}: FizzBuzz");
        } else if (i % 3 == 0) {
            println("{i}: Fizz");
        } else if (i % 5 == 0) {
            println("{i}: Buzz");
        } else {
            println("{i}");
        }
    }
    return 0;
}
