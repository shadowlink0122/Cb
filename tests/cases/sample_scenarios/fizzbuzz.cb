// FizzBuzz implementation test
int main() {
    for (int i = 1; i <= 15; i = i + 1) {
        if ((i % 3 == 0) && (i % 5 == 0)) {
            println("%d: FizzBuzz", i);
        } else if (i % 3 == 0) {
            println("%d: Fizz", i);
        } else if (i % 5 == 0) {
            println("%d: Buzz", i);
        } else {
            println("%d", i);
        }
    }
    return 0;
}
