// Mixed output format testing (print vs println)
int main() {
    // Test various output combinations
    print("Start");
    println();
    
    println("Line 1");
    print("Inline: ");
    println("continuation");
    
    // Printf-style formatting
    int value = 42;
    string name = "Test";
    
    println("Value: {value}");
    println("Name: {name}");
    println("Combined: {name} has value {value}");
    
    // Mixed print and println
    print("A");
    print("B");
    print("C");
    println();
    println("NewLine");
    
    return 0;
}
