// Namespace機能の包括的テスト
namespace math {
    int add(int a, int b) {
        return a + b;
    }
    
    int multiply(int a, int b) {
        return a * b;
    }
    
    namespace advanced {
        int power(int base, int exp) {
            int result = 1;
            for (int i = 0; i < exp; i = i + 1) {
                result = result * base;
            }
            return result;
        }
    }
}

namespace utils {
    int max(int a, int b) {
        if (a > b) {
            return a;
        } else {
            return b;
        }
    }
}

using namespace math;
using namespace utils;

int main() {
    // using namespaceで非修飾名呼び出し
    int sum = add(5, 3);              // 8
    int product = multiply(4, 3);     // 12
    int maximum = max(10, 20);        // 20
    
    // 修飾名での呼び出し
    int sum2 = math::add(10, 5);      // 15
    int power_result = math::advanced::power(2, 3);  // 8 (2^3)
    
    print(sum);          // 8
    print(product);      // 12
    print(maximum);      // 20
    print(sum2);         // 15
    print(power_result); // 8
    
    return 0;
}
