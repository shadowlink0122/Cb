// 配列リテラル初期化テスト
int main() {
    println("=== Array Literal Initialization Test ===");
    
    // 標準配列の[]記法初期化
    int numbers[] = [10, 20, 30, 40];
    string words[] = ["hello", "world", "test"];
    bool flags[] = [true, false, true];
    
    println("Standard array initialization with [] syntax:");
    
    println("int numbers[] = [10, 20, 30, 40]:");
    print("  numbers[0] = ");
    print(numbers[0]);
    println("");
    print("  numbers[1] = ");
    print(numbers[1]);
    println("");
    print("  numbers[2] = ");
    print(numbers[2]);
    println("");
    print("  numbers[3] = ");
    print(numbers[3]);
    println("");
    
    println("string words[] = [\"hello\", \"world\", \"test\"]:");
    print("  words[0] = ");
    print(words[0]);
    println("");
    print("  words[1] = ");
    print(words[1]);
    println("");
    print("  words[2] = ");
    print(words[2]);
    println("");
    
    println("bool flags[] = [true, false, true]:");
    print("  flags[0] = ");
    print(flags[0]);
    println("");
    print("  flags[1] = ");
    print(flags[1]);
    println("");
    print("  flags[2] = ");
    print(flags[2]);
    println("");
    
    println("Array literal initialization test completed!");
    return 0;
}
