// 構造体ポインタの最小テスト

struct Point {
    int x;
    int y;
};

int main() {
    Point p = {x: 10, y: 20};
    println(p.x);  // 10
    println(p.y);  // 20
    
    // まず int* が動作するか確認
    int a = 5;
    int* pa = &a;
    println(*pa);  // 5
    
    // 次に Point* を試す
    Point* pp = &p;
    println((*pp).x);  // 10
    println((*pp).y);  // 20
    
    return 0;
}
