// typedef複雑なテスト
typedef int Counter;
typedef string Name;

void process_data(Counter count, Name identifier) {
    print("Processing: ");
    print(identifier);
    print(" with count: ");
    print(count);
    println();
}

int main() {
    println("=== Advanced Typedef Test ===");
    
    Counter items;
    Name category;
    items = 10;
    category = "Electronics";
    
    process_data(items, category);
    
    // 型エイリアスでの演算
    Counter total;
    total = items * 2;
    print("Total after doubling: ");
    print(total);
    println();
    
    // 型エイリアスでの条件分岐
    if (total > 15) {
        println("High quantity detected");
    } else {
        println("Low quantity");
    }
    
    println("");
    println("Advanced typedef test completed!");
    return 0;
}
