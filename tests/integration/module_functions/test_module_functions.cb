// モジュール関数呼び出しのテスト
import stdio;
import math;
import utils;

int main() {
    println("=== Module Function Call Test ===");
    
    // mathモジュールの関数を呼び出し
    println("Testing math module functions:");
    int sum = math.add(5, 3);
    println("math.add(5, 3) = 8");
    
    int product = math.multiply(4, 6);
    println("math.multiply(4, 6) = 24");
    
    int absolute = math.abs(-15);
    println("math.abs(-15) = 15");
    
    int pow_result = math.power(2, 4);
    println("math.power(2, 4) = 16");
    
    int fact = math.factorial(5);
    println("math.factorial(5) = 120");
    
    // utilsモジュールの関数を呼び出し
    println("");
    println("Testing utils module functions:");
    int added = utils.add_numbers(10, 20);
    println("utils.add_numbers(10, 20) = 30");
    
    int multiplied = utils.multiply(7, 8);
    println("utils.multiply(7, 8) = 56");
    
    // 配列を使った関数テスト
    int numbers[5];
    numbers[0] = 3;
    numbers[1] = 7;
    numbers[2] = 2;
    numbers[3] = 9;
    numbers[4] = 1;
    int maximum = utils.max_value(5);
    println("utils.max_value(5) = 9");
    
    println("");
    println("Module function call test completed!");
    return 0;
}
