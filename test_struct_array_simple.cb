// 構造体型配列メンバーの単純な個別代入テスト

struct Point {
    int x;
    int y;
};

struct Data {
    Point[2] points;
};

int main() {
    Data d;
    
    println("Before assignment:");
    println("points[0] = (%d, %d)", d.points[0].x, d.points[0].y);
    println("points[1] = (%d, %d)", d.points[1].x, d.points[1].y);
    
    d.points[0].x = 10;
    println("\nAfter d.points[0].x = 10:");
    println("points[0] = (%d, %d)", d.points[0].x, d.points[0].y);
    
    d.points[0].y = 20;
    println("\nAfter d.points[0].y = 20:");
    println("points[0] = (%d, %d)", d.points[0].x, d.points[0].y);
    
    d.points[1].x = 30;
    d.points[1].y = 40;
    println("\nAfter d.points[1] assignment:");
    println("points[1] = (%d, %d)", d.points[1].x, d.points[1].y);
    
    println("\nFinal check:");
    println("points[0] = (%d, %d)", d.points[0].x, d.points[0].y);
    println("points[1] = (%d, %d)", d.points[1].x, d.points[1].y);
    
    return 0;
}
