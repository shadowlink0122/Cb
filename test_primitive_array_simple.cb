// プリミティブ型配列メンバーの単純な個別代入テスト

struct Data {
    int[3] values;
};

int main() {
    Data d;
    
    println("Before assignment:");
    println("values[0] = %d", d.values[0]);
    println("values[1] = %d", d.values[1]);
    println("values[2] = %d", d.values[2]);
    
    d.values[0] = 10;
    println("\nAfter d.values[0] = 10:");
    println("values[0] = %d", d.values[0]);
    
    d.values[1] = 20;
    println("\nAfter d.values[1] = 20:");
    println("values[1] = %d", d.values[1]);
    
    d.values[2] = 30;
    println("\nAfter d.values[2] = 30:");
    println("values[2] = %d", d.values[2]);
    
    println("\nFinal check:");
    println("values[0] = %d", d.values[0]);
    println("values[1] = %d", d.values[1]);
    println("values[2] = %d", d.values[2]);
    
    return 0;
}
