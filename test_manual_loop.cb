struct Point {
    int x;
    int y;
};

void main() {
    println("=== Manual Loop Test ===");
    
    Point[3] points;
    
    int i;
    i = 0;
    {
        println("Setting points[0]");
        points[i].x = i * 10;
        points[i].y = i * 20;
        println("points[0].x = {points[0].x}, points[0].y = {points[0].y}");
    }
    
    i = 1;
    {
        println("Setting points[1]");
        points[i].x = i * 10;
        points[i].y = i * 20;
        println("points[1].x = {points[1].x}, points[1].y = {points[1].y}");
    }
    
    println("\nAfter manual loop:");
    println("points[0].x = {points[0].x}, points[0].y = {points[0].y}");
    println("points[1].x = {points[1].x}, points[1].y = {points[1].y}");
}
