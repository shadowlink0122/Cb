<span style="color: #6a9955;">// å‹ â†’ interface â†’ ãƒ¡ã‚½ãƒƒãƒ‰å®Ÿè£… ã®ãƒãƒƒãƒ—</span>
<span style="color: #4ec9b0;">std::map</span>&lt;<span style="color: #4ec9b0;">TypeInfo</span>, <span style="color: #4ec9b0;">std::map</span>&lt;<span style="color: #4ec9b0;">std::string</span>, <span style="color: #4ec9b0;">ASTNode</span>*&gt;&gt; interface_impls;

<span style="color: #6a9955;">// ãƒ¡ã‚½ãƒƒãƒ‰å‘¼ã³å‡ºã—ã®è§£æ±º</span>
<span style="color: #4ec9b0;">ASTNode</span>* <span style="color: #dcdcaa;">resolve_method</span>(<span style="color: #4ec9b0;">TypeInfo</span> type, <span style="color: #4ec9b0;">std::string</span> method_name) {
    <span style="color: #c586c0;">return</span> interface_impls[type][method_name];  <span style="color: #6a9955;">// O(log n)ã§æ¤œç´¢</span>
}</code></pre>
