            <!-- æ–°ã‚¹ãƒ©ã‚¤ãƒ‰: å”èª¿çš„ãƒãƒ«ãƒã‚¿ã‚¹ã‚¯ã®å®Ÿè£… (1/2) -->
            <section class="left-align">
                <h2>ã‚¤ãƒ³ã‚¿ãƒ—ãƒªã‚¿å†…éƒ¨å®Ÿè£…ï¼ˆ2ï¼‰: å”èª¿çš„ãƒãƒ«ãƒã‚¿ã‚¹ã‚¯ï¼ˆ1/2ï¼‰</h2>
                <div style="width: 95%; margin: 0.2em auto 0;">
                    <h3 style="font-size: 0.7em; margin-bottom: 0.15em;">ã‚¤ãƒ™ãƒ³ãƒˆãƒ«ãƒ¼ãƒ—ã®åŸºæœ¬æ§‹é€ </h3>
                    <pre style="font-size: 0.38em; margin: 0.2em 0;"><code class="language-cpp" data-trim data-noescape><span style="color: #6a9955;">// Task: å®Ÿè¡Œå¯èƒ½ãªéåŒæœŸå‡¦ç†ã®å˜ä½</span>
<span style="color: #c586c0;">struct</span> <span style="color: #4ec9b0;">Task</span> {
    <span style="color: #4ec9b0;">ASTNode</span>* ast;              <span style="color: #6a9955;">// å®Ÿè¡Œã™ã‚‹AST</span>
    <span style="color: #4ec9b0;">Scope</span>* scope;              <span style="color: #6a9955;">// ã‚¹ã‚³ãƒ¼ãƒ—æƒ…å ±</span>
    <span style="color: #4ec9b0;">TaskState</span> state;           <span style="color: #6a9955;">// Ready, Running, Suspended, Completed</span>
    <span style="color: #4ec9b0;">Value</span> result;              <span style="color: #6a9955;">// å®Ÿè¡Œçµæœ</span>
    <span style="color: #4ec9b0;">long</span> resume_time_ms;       <span style="color: #6a9955;">// sleepæ™‚ã®å†é–‹æ™‚åˆ»ï¼ˆãƒŸãƒªç§’ï¼‰</span>
    <span style="color: #4ec9b0;">std::vector</span>&lt;<span style="color: #4ec9b0;">Task</span>*&gt; deps;  <span style="color: #6a9955;">// ä¾å­˜ã‚¿ã‚¹ã‚¯ï¼ˆawaitå¯¾è±¡ï¼‰</span>
};

<span style="color: #6a9955;">// ã‚¤ãƒ™ãƒ³ãƒˆãƒ«ãƒ¼ãƒ—: å…¨ã‚¿ã‚¹ã‚¯ã‚’ç®¡ç†</span>
<span style="color: #c586c0;">class</span> <span style="color: #4ec9b0;">EventLoop</span> {
    <span style="color: #4ec9b0;">std::queue</span>&lt;<span style="color: #4ec9b0;">Task</span>*&gt; ready_queue;      <span style="color: #6a9955;">// å®Ÿè¡Œå¯èƒ½ãªã‚¿ã‚¹ã‚¯</span>
    <span style="color: #4ec9b0;">std::vector</span>&lt;<span style="color: #4ec9b0;">Task</span>*&gt; suspended_tasks; <span style="color: #6a9955;">// ä¸€æ™‚åœæ­¢ä¸­ã®ã‚¿ã‚¹ã‚¯</span>
    <span style="color: #4ec9b0;">long</span> current_time_ms;                 <span style="color: #6a9955;">// ç¾åœ¨æ™‚åˆ»ï¼ˆä»®æƒ³æ™‚é–“ï¼‰</span>

<span style="color: #c586c0;">public</span>:
    <span style="color: #4ec9b0;">void</span> <span style="color: #dcdcaa;">run</span>() {
        <span style="color: #c586c0;">while</span> (!ready_queue<span style="color: #dcdcaa;">.empty</span>() || !suspended_tasks<span style="color: #dcdcaa;">.empty</span>()) {
            <span style="color: #6a9955;">// 1. sleepä¸­ã®ã‚¿ã‚¹ã‚¯ã‚’ãƒã‚§ãƒƒã‚¯</span>
            <span style="color: #dcdcaa;">check_sleeping_tasks</span>();
            
            <span style="color: #6a9955;">// 2. å®Ÿè¡Œå¯èƒ½ãªã‚¿ã‚¹ã‚¯ã‚’1ã¤å–ã‚Šå‡ºã—ã¦å®Ÿè¡Œ</span>
            <span style="color: #c586c0;">if</span> (!ready_queue<span style="color: #dcdcaa;">.empty</span>()) {
                <span style="color: #4ec9b0;">Task</span>* task = ready_queue<span style="color: #dcdcaa;">.front</span>();
                ready_queue<span style="color: #dcdcaa;">.pop</span>();
                <span style="color: #dcdcaa;">execute_task</span>(task);  <span style="color: #6a9955;">// yield/awaitã¾ã§å®Ÿè¡Œ</span>
            } <span style="color: #c586c0;">else</span> {
                <span style="color: #6a9955;">// å®Ÿè¡Œå¯èƒ½ãªã‚¿ã‚¹ã‚¯ãŒãªã„å ´åˆã€æ™‚é–“ã‚’é€²ã‚ã‚‹</span>
                <span style="color: #dcdcaa;">advance_time</span>();
            }
        }
    }
};</code></pre>
                </div>
            </section>

            <!-- æ–°ã‚¹ãƒ©ã‚¤ãƒ‰: å”èª¿çš„ãƒãƒ«ãƒã‚¿ã‚¹ã‚¯ã®å®Ÿè£… (2/2) -->
            <section class="left-align">
                <h2>ã‚¤ãƒ³ã‚¿ãƒ—ãƒªã‚¿å†…éƒ¨å®Ÿè£…ï¼ˆ2ï¼‰: å”èª¿çš„ãƒãƒ«ãƒã‚¿ã‚¹ã‚¯ï¼ˆ2/2ï¼‰</h2>
                <div style="width: 95%; margin: 0.2em auto 0;">
                    <h3 style="font-size: 0.7em; margin-bottom: 0.15em;">await/sleepã®å‹•ä½œ</h3>
                    <pre style="font-size: 0.38em; margin: 0.2em 0;"><code class="language-cpp" data-trim data-noescape><span style="color: #6a9955;">// await: ä»–ã®ã‚¿ã‚¹ã‚¯ã®å®Œäº†ã‚’å¾…ã¤</span>
<span style="color: #4ec9b0;">void</span> <span style="color: #dcdcaa;">execute_await</span>(<span style="color: #4ec9b0;">Task</span>* current_task, <span style="color: #4ec9b0;">Task</span>* target_task) {
    <span style="color: #c586c0;">if</span> (target_task->state == <span style="color: #4ec9b0;">TaskState</span>::Completed) {
        <span style="color: #6a9955;">// æ—¢ã«å®Œäº†: å³åº§ã«çµæœã‚’è¿”ã™</span>
        current_task->result = target_task->result;
    } <span style="color: #c586c0;">else</span> {
        <span style="color: #6a9955;">// æœªå®Œäº†: ç¾åœ¨ã®ã‚¿ã‚¹ã‚¯ã‚’ä¸€æ™‚åœæ­¢ã—ã¦ã€ä¾å­˜é–¢ä¿‚ã‚’ç™»éŒ²</span>
        current_task->state = <span style="color: #4ec9b0;">TaskState</span>::Suspended;
        current_task->deps<span style="color: #dcdcaa;">.push_back</span>(target_task);
        suspended_tasks<span style="color: #dcdcaa;">.push_back</span>(current_task);
        <span style="color: #6a9955;">// â†’ ä»–ã®ã‚¿ã‚¹ã‚¯ãŒå®Ÿè¡Œã•ã‚Œã‚‹ï¼ˆå”èª¿çš„ãƒãƒ«ãƒã‚¿ã‚¹ã‚¯ï¼‰</span>
    }
}

<span style="color: #6a9955;">// sleep: æŒ‡å®šæ™‚é–“ã ã‘ã‚¿ã‚¹ã‚¯ã‚’åœæ­¢ï¼ˆä»–ã®ã‚¿ã‚¹ã‚¯ã‚’ãƒ–ãƒ­ãƒƒã‚¯ã—ãªã„ï¼‰</span>
<span style="color: #4ec9b0;">void</span> <span style="color: #dcdcaa;">execute_sleep</span>(<span style="color: #4ec9b0;">Task</span>* task, <span style="color: #4ec9b0;">long</span> duration_ms) {
    task->state = <span style="color: #4ec9b0;">TaskState</span>::Suspended;
    task->resume_time_ms = current_time_ms + duration_ms;  <span style="color: #6a9955;">// å†é–‹æ™‚åˆ»ã‚’è¨­å®š</span>
    suspended_tasks<span style="color: #dcdcaa;">.push_back</span>(task);
    <span style="color: #6a9955;">// â†’ ä»–ã®ã‚¿ã‚¹ã‚¯ãŒå®Ÿè¡Œã•ã‚Œã‚‹ï¼ˆsleepã¯ä»–ã‚’ãƒ–ãƒ­ãƒƒã‚¯ã—ãªã„ï¼ï¼‰</span>
}

<span style="color: #6a9955;">// æ™‚é–“çµŒéãƒã‚§ãƒƒã‚¯: sleepä¸­ã®ã‚¿ã‚¹ã‚¯ã‚’å†é–‹</span>
<span style="color: #4ec9b0;">void</span> <span style="color: #dcdcaa;">check_sleeping_tasks</span>() {
    <span style="color: #c586c0;">for</span> (<span style="color: #c586c0;">auto</span> it = suspended_tasks<span style="color: #dcdcaa;">.begin</span>(); it != suspended_tasks<span style="color: #dcdcaa;">.end</span>();) {
        <span style="color: #c586c0;">if</span> ((*it)->resume_time_ms &lt;= current_time_ms) {
            <span style="color: #6a9955;">// æ™‚é–“ãŒæ¥ãŸã®ã§å†é–‹</span>
            (*it)->state = <span style="color: #4ec9b0;">TaskState</span>::Ready;
            ready_queue<span style="color: #dcdcaa;">.push</span>(*it);
            it = suspended_tasks<span style="color: #dcdcaa;">.erase</span>(it);
        } <span style="color: #c586c0;">else</span> {
            ++it;
        }
    }
}</code></pre>
                </div>
            </section>

            <!-- æ–°ã‚¹ãƒ©ã‚¤ãƒ‰: éåŒæœŸå‡¦ç†ã¨sleep -->
            <section class="left-align">
                <h2>éåŒæœŸå‡¦ç†ã¨sleep - ãƒ–ãƒ­ãƒƒã‚­ãƒ³ã‚°ã—ãªã„å®Ÿè¡Œ</h2>
                <div style="display: flex; gap: 1.0em; width: 95%; margin: 0.3em auto 0;">
                    <div style="flex: 1;">
                        <h3 style="font-size: 0.7em; margin-bottom: 0.15em;">sleepã®å‹•ä½œ</h3>
                        <pre style="font-size: 0.39em; margin: 0.2em 0;"><code class="language-cpp" data-trim data-noescape><span style="color: #c586c0;">async</span> <span style="color: #4ec9b0;">void</span> task1() {
    println(<span style="color: #ce9178;">"Task1: Start"</span>);
    sleep(<span style="color: #b5cea8;">1000</span>);  <span style="color: #6a9955;">// 1ç§’å¾…æ©Ÿï¼ˆä»–ã®ã‚¿ã‚¹ã‚¯ã¯ãƒ–ãƒ­ãƒƒã‚¯ã•ã‚Œãªã„ï¼‰</span>
    println(<span style="color: #ce9178;">"Task1: After 1s"</span>);
}

<span style="color: #c586c0;">async</span> <span style="color: #4ec9b0;">void</span> task2() {
    println(<span style="color: #ce9178;">"Task2: Start"</span>);
    sleep(<span style="color: #b5cea8;">500</span>);   <span style="color: #6a9955;">// 0.5ç§’å¾…æ©Ÿ</span>
    println(<span style="color: #ce9178;">"Task2: After 0.5s"</span>);
}

<span style="color: #4ec9b0;">void</span> main() {
    task1();
    task2();
}

<span style="color: #6a9955;">// å‡ºåŠ›é †åº:</span>
<span style="color: #6a9955;">// Task1: Start</span>
<span style="color: #6a9955;">// Task2: Start</span>
<span style="color: #6a9955;">// Task2: After 0.5s  â† task2ãŒå…ˆã«å®Œäº†</span>
<span style="color: #6a9955;">// Task1: After 1s</span></code></pre>

                        <h3 style="font-size: 0.7em; margin-bottom: 0.15em; margin-top: 0.25em;">æ™‚é–“çµŒéã®æ¦‚å¿µ</h3>
                        <ul style="font-size: 0.60em; line-height: 1.3; margin: 0; padding-left: 1.3em;">
                            <li>å„ã‚¿ã‚¹ã‚¯ã”ã¨ã«<strong>ä»®æƒ³æ™‚é–“</strong>ã‚’ç®¡ç†</li>
                            <li>sleepã¯æ™‚é–“ã‚’é€²ã‚ã‚‹ã ã‘ï¼ˆCPUã‚’ãƒ–ãƒ­ãƒƒã‚¯ã—ãªã„ï¼‰</li>
                            <li>å®Ÿè¡Œå¯èƒ½ãªã‚¿ã‚¹ã‚¯ãŒãªã„å ´åˆã€æ™‚é–“ã‚’æœ€å°ã®resume_timeã¾ã§é€²ã‚ã‚‹</li>
                        </ul>
                    </div>
                    <div style="flex: 1;">
                        <h3 style="font-size: 0.7em; margin-bottom: 0.15em;">concurrent vs sequential</h3>
                        <pre style="font-size: 0.39em; margin: 0.2em 0;"><code class="language-cpp" data-trim data-noescape><span style="color: #6a9955;">// Concurrentå®Ÿè¡Œ: ä¸¦è¡Œå®Ÿè¡Œï¼ˆawaitä¸ä½¿ç”¨ï¼‰</span>
<span style="color: #4ec9b0;">void</span> main() {
    task1();  <span style="color: #6a9955;">// ã‚¿ã‚¹ã‚¯ã‚’é–‹å§‹ï¼ˆå¾…ãŸãªã„ï¼‰</span>
    task2();  <span style="color: #6a9955;">// ã‚¿ã‚¹ã‚¯ã‚’é–‹å§‹ï¼ˆå¾…ãŸãªã„ï¼‰</span>
    <span style="color: #6a9955;">// â†’ ã‚¤ãƒ™ãƒ³ãƒˆãƒ«ãƒ¼ãƒ—ãŒä¸¡æ–¹ã‚’ä¸¦è¡Œå®Ÿè¡Œ</span>
}

<span style="color: #6a9955;">// Sequentialå®Ÿè¡Œ: é€æ¬¡å®Ÿè¡Œï¼ˆawaitä½¿ç”¨ï¼‰</span>
<span style="color: #c586c0;">async</span> <span style="color: #4ec9b0;">void</span> main() {
    <span style="color: #c586c0;">await</span> task1();  <span style="color: #6a9955;">// task1ã®å®Œäº†ã‚’å¾…ã¤</span>
