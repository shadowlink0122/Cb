            <!-- ã‚¹ãƒ©ã‚¤ãƒ‰: å”èª¿çš„ãƒãƒ«ãƒã‚¿ã‚¹ã‚¯ã®å®Ÿè£… - Part 1 -->
            <section class="left-align">
                <h2>å”èª¿çš„ãƒãƒ«ãƒã‚¿ã‚¹ã‚¯ï¼ˆasync/awaitï¼‰ã®å®Ÿè£… - Part 1</h2>
                <div style="width: 95%; margin: 0.2em auto 0;">
                    <div style="display: flex; gap: 0.8em; margin-bottom: 0.35em;">
                        <div style="flex: 1;">
                            <h3 style="font-size: 0.68em; margin-bottom: 0.15em;">ã‚¢ãƒ¼ã‚­ãƒ†ã‚¯ãƒãƒ£æ¦‚è¦</h3>
                            <ul style="font-size: 0.58em; line-height: 1.25; margin: 0; padding-left: 1.2em;">
                                <li><strong>Event Loop</strong>: ã‚¿ã‚¹ã‚¯ã‚­ãƒ¥ãƒ¼ç®¡ç†</li>
                                <li><strong>Future&lt;T&gt;</strong>: éåŒæœŸã‚¿ã‚¹ã‚¯ã®çŠ¶æ…‹ã‚’è¡¨ç¾</li>
                                <li><strong>await</strong>: åˆ¶å¾¡ã‚’Event Loopã«æˆ»ã™</li>
                                <li><strong>å”èª¿çš„ã‚¹ã‚±ã‚¸ãƒ¥ãƒ¼ãƒªãƒ³ã‚°</strong>: ã‚¿ã‚¹ã‚¯è‡ªèº«ãŒåˆ¶å¾¡ã‚’è­²æ¸¡</li>
                            </ul>
                        </div>
                        <div style="flex: 1;">
                            <h3 style="font-size: 0.68em; margin-bottom: 0.15em;">Future&lt;T&gt;ã®çŠ¶æ…‹ç®¡ç†</h3>
                            <pre style="font-size: 0.36em; margin: 0.25em 0;"><code class="language-cpp" data-trim>
<span style="color: #c586c0; font-weight: bold;">enum</span> <span style="color: #4ec9b0;">FutureState</span> {
    PENDING,    <span style="color: #6a9955;">// å®Ÿè¡Œä¸­</span>
    READY,      <span style="color: #6a9955;">// å®Œäº†</span>
    WAITING     <span style="color: #6a9955;">// å¾…æ©Ÿä¸­ï¼ˆsleepç­‰ï¼‰</span>
};

<span style="color: #c586c0; font-weight: bold;">struct</span> <span style="color: #4ec9b0;">Future</span>&lt;<span style="color: #4ec9b0;">T</span>&gt; {
    <span style="color: #4ec9b0;">FutureState</span> state;
    <span style="color: #4ec9b0;">T</span> value;          <span style="color: #6a9955;">// çµæœå€¤</span>
    <span style="color: #4ec9b0;">long</span> wake_time;   <span style="color: #6a9955;">// å†é–‹æ™‚åˆ»</span>
    <span style="color: #4ec9b0;">ASTNode</span>* continuation;
};
                            </code></pre>
                        </div>
                    </div>
                    
                    <h3 style="font-size: 0.68em; margin-bottom: 0.15em;">Event Loopã®ã‚¯ãƒ©ã‚¹å®šç¾©</h3>
                    <pre style="font-size: 0.40em; margin: 0.25em 0;"><code class="language-cpp" data-trim>
<span style="color: #c586c0; font-weight: bold;">class</span> <span style="color: #4ec9b0;">EventLoop</span> {
    std::<span style="color: #4ec9b0;">deque</span>&lt;<span style="color: #4ec9b0;">Task</span>*&gt; task_queue;
    std::<span style="color: #4ec9b0;">map</span>&lt;<span style="color: #4ec9b0;">int</span>, <span style="color: #4ec9b0;">Future</span>*&gt; pending_futures;
    <span style="color: #4ec9b0;">int</span> next_task_id = <span style="color: #b5cea8;">0</span>;

<span style="color: #c586c0; font-weight: bold;">public</span>:
    <span style="color: #6a9955;">// asyncé–¢æ•°ã‚’å®Ÿè¡Œ</span>
    <span style="color: #4ec9b0;">int</span> spawn_task(<span style="color: #4ec9b0;">ASTNode</span>* async_func, <span style="color: #4ec9b0;">Scope</span>& scope) {
        <span style="color: #4ec9b0;">Task</span>* task = <span style="color: #c586c0; font-weight: bold;">new</span> <span style="color: #4ec9b0;">Task</span>{next_task_id++, async_func, scope};
        task_queue.<span style="color: #dcdcaa;">push_back</span>(task);
        <span style="color: #c586c0; font-weight: bold;">return</span> task->id;
    }
    
    <span style="color: #6a9955;">// awaitå¼ã®è©•ä¾¡</span>
    <span style="color: #4ec9b0;">TypedValue</span> evaluate_await(<span style="color: #4ec9b0;">ASTNode</span>* await_expr, <span style="color: #4ec9b0;">Scope</span>& scope) {
        <span style="color: #6a9955;">// 1. awaitã™ã‚‹å¼ã‚’è©•ä¾¡</span>
        <span style="color: #4ec9b0;">TypedValue</span> future_value = evaluate(await_expr->child, scope);
        
        <span style="color: #6a9955;">// 2. FutureãŒã¾ã å®Œäº†ã—ã¦ã„ãªã„å ´åˆ</span>
        <span style="color: #c586c0; font-weight: bold;">if</span> (future_value.<span style="color: #dcdcaa;">is_pending</span>()) {
            <span style="color: #c586c0; font-weight: bold;">return</span> <span style="color: #4ec9b0;">TypedValue</span>::<span style="color: #4ec9b0;">Pending</span>();  <span style="color: #6a9955;">// Event Loopã«åˆ¶å¾¡ã‚’æˆ»ã™</span>
        }
        
        <span style="color: #6a9955;">// 3. FutureãŒå®Œäº†ã—ã¦ã„ã‚‹ â†’ å€¤ã‚’å–ã‚Šå‡ºã™</span>
        <span style="color: #c586c0; font-weight: bold;">return</span> future_value.<span style="color: #dcdcaa;">unwrap</span>();  <span style="color: #6a9955;">// Result/Optionã®enumæƒ…å ±ã‚’ä¿æŒ</span>
    }
};
                    </code></pre>
                </div>
            </section>

            <!-- ã‚¹ãƒ©ã‚¤ãƒ‰: å”èª¿çš„ãƒãƒ«ãƒã‚¿ã‚¹ã‚¯ã®å®Ÿè£… - Part 2 -->
            <section class="left-align">
                <h2>å”èª¿çš„ãƒãƒ«ãƒã‚¿ã‚¹ã‚¯ï¼ˆasync/awaitï¼‰ã®å®Ÿè£… - Part 2</h2>
                <div style="width: 95%; margin: 0.2em auto 0;">
                    <h3 style="font-size: 0.68em; margin-bottom: 0.15em;">Event Loopå®Ÿè¡Œãƒ­ã‚¸ãƒƒã‚¯</h3>
                    <pre style="font-size: 0.40em; margin: 0.25em 0;"><code class="language-cpp" data-trim>
<span style="color: #6a9955;">// Event Loopã‚’å®Ÿè¡Œ</span>
<span style="color: #4ec9b0;">void</span> EventLoop::run() {
    <span style="color: #c586c0; font-weight: bold;">while</span> (!task_queue.<span style="color: #dcdcaa;">empty</span>()) {
        <span style="color: #4ec9b0;">Task</span>* task = task_queue.<span style="color: #dcdcaa;">front</span>();
        task_queue.<span style="color: #dcdcaa;">pop_front</span>();
        
        <span style="color: #6a9955;">// awaitå¼ã‚’è©•ä¾¡ï¼ˆåˆ¶å¾¡ã‚’æˆ»ã™å¯èƒ½æ€§ã‚ã‚Šï¼‰</span>
        <span style="color: #4ec9b0;">TypedValue</span> result = evaluate_task(task);
        
        <span style="color: #c586c0; font-weight: bold;">if</span> (result.<span style="color: #dcdcaa;">is_pending</span>()) {
            <span style="color: #6a9955;">// ã¾ã å®Œäº†ã—ã¦ã„ãªã„ â†’ ã‚­ãƒ¥ãƒ¼ã«æˆ»ã™</span>
            <span style="color: #c586c0; font-weight: bold;">if</span> (current_time_ms() >= task->wake_time) {
                task_queue.<span style="color: #dcdcaa;">push_back</span>(task);  <span style="color: #6a9955;">// ã™ãå†å®Ÿè¡Œ</span>
            } <span style="color: #c586c0; font-weight: bold;">else</span> {
                task_queue.<span style="color: #dcdcaa;">push_back</span>(task);  <span style="color: #6a9955;">// å¾Œã§å†ãƒã‚§ãƒƒã‚¯</span>
            }
        } <span style="color: #c586c0; font-weight: bold;">else</span> {
            <span style="color: #6a9955;">// å®Œäº† â†’ Futureã‚’è§£æ±º</span>
            resolve_future(task->id, result);
            <span style="color: #c586c0; font-weight: bold;">delete</span> task;
        }
    }
}
