    <span style="color: #4ec9b0;">std::vector</span>&lt;<span style="color: #4ec9b0;">TypeInfo</span>&gt; params;  <span style="color: #6a9955;">// ãƒ‘ãƒ©ãƒ¡ãƒ¼ã‚¿ã®å‹ãƒªã‚¹ãƒˆ</span>
