// 値を返すケース
int increment_and_return() {
    static int count = 0;
    println("Before increment: %d", count);
    count++;
    println("After increment: %d", count);
    println("About to return count");
    return count;
}

int main() {
    println("Calling function...");
    int result = increment_and_return();
    println("Result: %d", result);
    return 0;
}
