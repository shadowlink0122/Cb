// ポインタを使ったスワップ関数のテスト

void swap(int* a, int* b) {
    int temp = *a;
    *a = *b;
    *b = temp;
}

int main() {
    int x = 10;
    int y = 20;
    
    println("Before swap:");
    println("x = %d, y = %d", x, y);
    
    swap(&x, &y);
    
    println("After swap:");
    println("x = %d, y = %d", x, y);
    
    return 0;
}
