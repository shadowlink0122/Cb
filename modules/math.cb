// modules/math.cb - 数学関数モジュール

// 定数のexport
export const int PI = 3;
export const int E = 2;

// 関数のexport
export int add(int a, int b) {
    return a + b;
}

export int multiply(int a, int b) {
    return a * b;
}

// デフォルトexport
export default int subtract(int a, int b) {
    return a - b;
}

// exportしない内部関数
int helper(int x) {
    return x * 2;
}
