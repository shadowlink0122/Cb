// モジュール変数の参照テスト
import stdio;
import math;

int main() {
    // モジュール関数呼び出しのテスト
    int sum = math.add(5, 3);
    print("math.add(5, 3) = %d\n", sum);
    
    // モジュール変数の参照テスト
    print("math.PI = %d\n", math.PI);
    print("math.E = %d\n", math.E);
    
    // 計算にモジュール変数を使用
    int area = math.multiply(math.PI, 2);
    print("area = PI * 2 = %d\n", area);
    
    return 0;
}
