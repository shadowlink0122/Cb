// Test Vector + Queue destructors together
import "stdlib/collections/vector.cb";
import "stdlib/collections/queue.cb";

void main() {
    println("Creating containers...");
    {
        Vector<int> vec;
        vec.init(5);
        vec.push(1);
        println("Vector created");
        
        Queue<int> q;
        q.enqueue(10);
        println("Queue created");
    }
    println("Scope exited - destructors called");
    println("Done");
}
