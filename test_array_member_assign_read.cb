// 配列メンバーへの代入と読み取りテスト

struct Student {
    string name;
    int[3] scores;
};

int main() {
    Student s;
    s.name = "Test";
    println("Before assignment: scores[0] = %d", s.scores[0]);
    
    s.scores[0] = 85;
    println("After assignment: scores[0] = %d", s.scores[0]);
    
    s.scores[1] = 90;
    s.scores[2] = 78;
    println("All scores: [%d, %d, %d]", s.scores[0], s.scores[1], s.scores[2]);
    
    return 0;
}
