// test_stack_struct_pointer.cb
// スタック上の構造体ポインタのメンバーアクセステスト

struct Point {
    int x;
    int y;
};

int main() {
    println("=== Test stack struct pointer ===");
    
    // スタック上の構造体
    Point p;
    p.x = 100;
    p.y = 200;
    println("Direct access: x={p.x}, y={p.y}");
    
    // スタック上の構造体へのポインタ
    Point* ptr = &p;
    println("Pointer access: x={ptr->x}, y={ptr->y}");
    
    // ポインタ経由で変更
    ptr->x = 300;
    ptr->y = 400;
    println("After modification: x={ptr->x}, y={ptr->y}");
    println("Direct check: x={p.x}, y={p.y}");
    
    println("Test complete");
    return 0;
}
