{
  "name": "cb-language",
  "displayName": "Cb Language Support",
  "description": "Syntax highlighting and language support for the Cb programming language with preprocessor support",
  "version": "0.13.0",
  "publisher": "cb-lang",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "cb",
    "programming language",
    "syntax highlighting"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shadowlink0122/Cb"
  },
  "license": "MIT",
  "icon": "images/icon.png",
  "scripts": {
    "update-version": "node scripts/update-version.js",
    "verify-version": "node scripts/verify-version.js",
    "prepackage": "npm run verify-version",
    "package": "vsce package"
  },
  "contributes": {
    "languages": [
      {
        "id": "cb",
        "aliases": [
          "Cb",
          "cb"
        ],
        "extensions": [
          ".cb"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./images/icon.svg",
          "dark": "./images/icon.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "cb",
        "scopeName": "source.cb",
        "path": "./syntaxes/cb.tmLanguage.json"
      }
    ],
    "iconThemes": [
      {
        "id": "cb-icons",
        "label": "Cb Icons",
        "path": "./iconTheme.json"
      }
    ]
  }
}
