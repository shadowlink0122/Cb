// 多次元配列メンバーのリテラル初期化テスト

struct Matrix {
    int[2][3] data;  // 2x3の多次元配列
    string name;
};

int main() {
    // 多次元配列メンバーのリテラル初期化
    Matrix m = {
        [[1, 2, 3], [4, 5, 6]],
        "TestMatrix"
    };
    
    println("Matrix: %s", m.name);
    println("Row 0: [%d, %d, %d]", m.data[0][0], m.data[0][1], m.data[0][2]);
    println("Row 1: [%d, %d, %d]", m.data[1][0], m.data[1][1], m.data[1][2]);
    
    return 0;
}
