// ネストした構造体のリテラル初期化テスト（簡易版）

struct Point {
    int x;
    int y;
};

struct Line {
    Point start;
    Point end;
};

int main() {
    // ネストした構造体リテラル初期化
    Line line = {{0, 0}, {10, 10}};
    
    println("Start: (%d, %d)", line.start.x, line.start.y);
    println("End: (%d, %d)", line.end.x, line.end.y);
    
    return 0;
}
