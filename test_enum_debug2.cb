// Debug enum test - more detailed
typedef enum { SUCCESS = 0, ERROR = 1, PENDING = -1 } Status;

Status get_error() {
    return Status::ERROR;
}

int main() {
    println("=== Enum Debug Test ===");
    
    // Test 1: Direct initialization
    println("\nTest 1: Direct initialization");
    Status s1 = Status::ERROR;
    println("s1 =", s1);
    
    // Test 2: Function return
    println("\nTest 2: Function return");
    Status s2 = get_error();
    println("s2 =", s2);
    
    // Test 3: Compare values
    println("\nTest 3: Comparison");
    if (s1 == s2) {
        println("s1 == s2: TRUE");
    } else {
        println("s1 == s2: FALSE");
    }
    
    return 0;
}
